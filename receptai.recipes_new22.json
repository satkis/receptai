  "ingredients": [
    {
      "name": {
        "lt": "Baltosios pupelės (virtos arba konservuotos)"
      },
      "quantity": "500 g",
      "vital": true,
      "notes": "Jeigu naudojate sausas, pamirkykite ir išvirkite iš anksto"
    },
    {
      "name": {
        "lt": "Morka"
      },
      "quantity": "2 vnt.",
      "vital": true
    },
    {
      "name": {
        "lt": "Saliero stiebas"
      },
      "quantity": "2 vnt.",
      "vital": true
    },
    {
      "name": {
        "lt": "Svogūnas"
      },
      "quantity": "1 didelis",
      "vital": true
    },
    {
      "name": {
        "lt": "Česnako skiltelės"
      },
      "quantity": "3 vnt.",
      "vital": true
    },
    {
      "name": {
        "lt": "Konservuoti smulkinti pomidorai"
      },
      "quantity": "400 g",
      "vital": true
    },
    {
      "name": {
        "lt": "Daržovių sultinys"
      },
      "quantity": "1 l",
      "vital": true
    },
    {
      "name": {
        "lt": "Alyvuogių aliejus"
      },
      "quantity": "3 šaukštai",
      "vital": false
    },
    {
      "name": {
        "lt": "Džiovintas čiobrelis"
      },
      "quantity": "1 arbatinis šaukštelis",
      "vital": false
    },
    {
      "name": {
        "lt": "Rozmarinas"
      },
      "quantity": "1/2 arbatinio šaukštelio",
      "vital": false
    },
    {
      "name": {
        "lt": "Druska ir pipirai"
      },
      "quantity": "pagal skonį",
      "vital": true
    },
    {
      "name": {
        "lt": "Lauro lapas"
      },
      "quantity": "1 vnt.",
      "vital": false
    },
    {
      "name": {
        "lt": "Šviežios petražolės"
      },
      "quantity": "1 sauja",
      "vital": false,
      "notes": "Pabarstymui"
    },
    {
      "name": {
        "lt": "Rustinė duona"
      },
      "quantity": "6 riekės",
      "vital": false,
      "notes": "Patiekti su sriuba"
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite naudoti bet kokius grybų tipus - pievagrybius, šampinjonus ar mišrainius"
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Patiekalas skaniausia šiltas, bet galima valgyti ir atvėsusį"
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Laikykite šaldytuve iki 3 dienų, prieš valgant pašildykite mikrobangų krosnelėje"
      },
      "priority": 3
    }
  ],
  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Daržovių paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite puodą su alyvuogių aliejumi, suberkite smulkintą svogūną, morkas, salierą ir česnaką. Kepkite 7–10 min, kol daržovės suminkštės."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Ingredientų sujungimas"
      },
      "text": {
        "lt": "Įpilkite pomidorus, sultinį, pupeles, lauro lapą, čiobrelius, rozmariną, druską ir pipirus. Išmaišykite ir užvirinkite."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Virimas"
      },
      "text": {
        "lt": "Sumažinkite ugnį ir virkite 25–30 min., kol skoniai susijungs. Esant poreikiui, dalį sriubos sutrinkite trintuvu, kad būtų tirštesnė konsistencija."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite karštą su petražolėmis ir skrudinta rustine duona šone."
      }
    }
  ],
