  "instructions": [
    {
      "step": 1,
      "name": { "lt": "Riešutų mišinio paruošimas" },
      "text": {
        "lt": "Dideliame dubenyje sumaišykite graikinius riešutus, cinamoną, maltus gvazdikėlius ir cukrų. Atskirame inde ištirpinkite sviestą."
      }
    },
    {
      "step": 2,
      "name": { "lt": "Tešlos paruošimas" },
      "text": {
        "lt": "Kepimo formą gausiai ištepkite tirpintu sviestu. Dugne ištieskite filo tešlos lapą, aptepkite sviestu ir kartokite dar su mažiausiai penkiais sluoksniais."
      }
    },
    {
      "step": 3,
      "name": { "lt": "Riešutų sluoksnių dėjimas" },
      "text": {
        "lt": "Ant šešto tešlos sluoksnio užberkite riešutų mišinį. Sluoksniuokite tešlą ir riešutus tol, kol sunaudosite visą mišinį. Viršuje turi likti apie šeši tešlos lapai."
      }
    },
    {
      "step": 4,
      "name": { "lt": "Kepimas" },
      "text": {
        "lt": "Supjaustykite tešlą į deimanto formos gabalėlius. Kepkite 150 °C temperatūroje apie 1,5 valandos, kol paviršius gražiai paruduos."
      }
    },
    {
      "step": 5,
      "name": { "lt": "Sirupo paruošimas" },
      "text": {
        "lt": "Visus sirupo ingredientus užvirkite, virkite apie 10 minučių ir atvėsinkite. Atvėsusiu sirupu užpilkite ką tik iš orkaitės išimtą karštą baklavą."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Norėdami intensyvesnio skonio, į sirupą įdėkite šiek tiek natūralaus medaus."
      },
      "priority": 1
    }
  ],
