  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Supjaustykite įdarui skirtus ingredientus. Kiaušinius gerai išplakite, galite pridėti šiek tiek pieno, kad būtų plonesni."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Supilkite kiaušinius į kambario temperatūros keptuvę. Kaitinkite, nuolat maišydami, kol pradės formuotis kietas kiaušinis."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Apversti ir įdaryti"
      },
      "text": {
        "lt": "Kai kiaušiniai pasidaro pakankamai tvirti, apverskite juos. Ant viršaus uždėkite įdarą ir užlenkite kiaušinį."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Kai omletas iškepa, uždenkite įdarą ir patiekite šiltą."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite naudoti 3 kiaušinius vietoj 2 kiaušinių ir 2 baltymų."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Omletą galite kepti svieste, kad būtų dar skanesnis."
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Įdarą galima pasirinkti pagal norą ir sezoną."
