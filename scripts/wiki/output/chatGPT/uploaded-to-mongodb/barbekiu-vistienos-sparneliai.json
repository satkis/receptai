    {
      "step": 2,
      "name": { "lt": "Grilio paruošimas" },
      "text": { "lt": "Įkaitinkite anglių židinį ir paskirstykite anglis tolygiai. Uždenkite groteles." }
    },
    {
      "step": 3,
      "name": { "lt": "Kepimas" },
      "text": { "lt": "Sudėkite vištienos sparnelius ant grotelių ir kepkite, dažnai versdami, kol vidinė temperatūra pasieks 74°C." }
    },
    {
      "step": 4,
      "name": { "lt": "Pateikimas" },
      "text": { "lt": "Nuimkite nuo grilio, uždenkite aliuminio folija ir leiskite pailsėti 5 minutes prieš patiekiant." }
    }
