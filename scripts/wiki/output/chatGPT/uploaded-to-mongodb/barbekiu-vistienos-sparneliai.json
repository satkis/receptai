  "instructions": [
    {
      "step": 1,
      "name": { "lt": "Paruošimas" },
      "text": { "lt": "Sumaišykite visus prieskonius ir apvolioję jais sparnelius, aptepkite barbekiu padažu. Palikite marinuotis šaldytuve kuo ilgiau." }
