  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Pupelių paruošimas"
      },
      "text": {
        "lt": "Rūšiuokite pupeles, kad pašalintumėte nešvarumus, ir mirkykite jas drungname vandenyje apie 30–40 minučių. Po to nusausinkite."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Pupelių valymas"
      },
      "text": {
        "lt": "Užpilkite pupeles vandeniu ir švelniai trinkite rankomis, kad pašalintumėte odeles. Iškyla į paviršių, todėl jas lengvai nuimkite."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Sumaišykite pupeles su svogūnais ir pipirais, įpilkite truputį vandens ir maišykite, kol gausite vientisą tešlą. Ji turėtų būti tiršta, bet lengvai pilama."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos pagardinimas"
      },
      "text": {
        "lt": "Supilkite tešlą į dubenį, įberkite druską ir prieskonius pagal skonį, gerai išmaišykite."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų keptuvėje ir šaukštu įpilkite tešlą. Kepkite iki auksinės rudos spalvos, sukant keletą kartų, kad tolygiai iškeptų."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Pateikimas"
      },
      "text": {
        "lt": "Patiekite karštus pupelių rutuliukus su mėgstamais priedais, pavyzdžiui, duona ar keptomis bulvytėmis."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Rinkitės aliejų pagal savo skonį - tai gali būti žemės riešutų aliejus arba palmių aliejus."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Jei nenorite pašalinti pupelių odelių, galite jas palikti, kad išsaugotumėte daugiau maistinių medžiagų."
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Laikykite likučius šaldytuve sandariame inde iki 2–3 dienų."
      },
      "priority": 3
    }
  ],
