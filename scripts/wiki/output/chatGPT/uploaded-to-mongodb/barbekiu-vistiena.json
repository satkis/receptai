  "instructions": [
    {
      "step": 1,
      "name": { "lt": "Marinavimas" },
      "text": { "lt": "Sumaišykite visas marinato sudedamąsias dalis didelėje užsegamoje maišoje. Įdėkite vištieną ir palikite šaldytuve 2 valandoms." }
    },
    {
      "step": 2,
      "name": { "lt": "Padažo paruošimas" },
      "text": { "lt": "Sumaišykite padažo ingredientus ir užvirkite ant vidutinės ugnies, kol sumažės perpus." }
    },
    {
      "step": 3,
      "name": { "lt": "Grilio paruošimas" },
      "text": { "lt": "Greitai nuvalykite grilį su aliejuje pamirkytu rankšluosčiu, kad jis būtų paruoštas naudojimui." }
    },
    {
      "step": 4,
      "name": { "lt": "Prieskonių įtrinimas" },
      "text": { "lt": "Sumaišykite prieskonių mišinį ir gerai įtrinkite į vištieną. Palikite šaldytuve 30 minučių." }
    },
    {
      "step": 5,
      "name": { "lt": "Kepimas ant grilio" },
      "text": { "lt": "Kepkite vištieną ant įkaitinto anglies grilio, dažnai apversdami ir tepant padažu, apie 10 minučių, kol vidinė temperatūra pasieks 74°C krūtinėlėje ir 77°C tamsioje mėsoje." }
    },
    {
      "step": 6,
      "name": { "lt": "Patiekimas" },
      "text": { "lt": "Uždengite folija ir leiskite pailsėti 7 minutes. Likusį padažą virkite 5 minutes ir supilkite į išspaudžiamą butelį. Patiekite vištieną su padažu." }
    }
  ],
  "notes": [
