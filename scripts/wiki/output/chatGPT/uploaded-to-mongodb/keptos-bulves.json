  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Bulvės Paruošimas"
      },
      "text": {
        "lt": "Gerai nuplaukite bulvę ir keliose vietose subadykite jos odą šakute. Taip garai kepimo metu galės lengvai pasišalinti."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kepimas Ant Grotelių"
      },
      "text": {
        "lt": "Kepimas Ant Grotelių || Įvyniokite bulvę į aliuminio foliją ir kepkite ant vidutinės ugnies apie 45 minutes, kol ji taps minkšta ir sultinga."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Kepimas Mikrobanginėje"
      },
      "text": {
        "lt": "epimas Mikrobanginėje || Įvyniokite bulvę į mikrobangų krosnelei tinkamą plėvelę, padarykite kelias skylutes ir kepkite maždaug 8 minutes, kol suminkštės."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Kepimas Orkaitėje"
      },
      "text": {
        "lt": "Kepimas Orkaitėje || Orkaitę įkaitinkite iki 190°C. Sudėkite bulvę ant skardos ir kepkite apie 1 valandą, kol vidus taps kreminis."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Perpjaukite karštą bulvę, pagardinkite druska ir įdėkite truputį sviesto arba grietinės. Taip pat tinka sūris, žolelės, česnakinis padažas ar kiti mėgstami priedai."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Skani idėja – patiekti su aštriomis pupelėmis, sūriu, šonine ar avokado gabalėliais."
      },
      "priority": 1
    }
  ],
