  "instructions": [
    {
      "step": 1,
      "name": { "lt": "Orkaitės paruošimas" },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 175°C. Paruoškite 23 cm kepimo formą – ištepkite sviestu ir pabarstykite miltais."
      }
    },
    {
      "step": 2,
      "name": { "lt": "Sausų ingredientų paruošimas" },
      "text": {
        "lt": "Dubenyje sumaišykite miltus, kepimo miltelius, sodą, druską ir cinamoną. Atidėkite į šalį."
      }
    },
    {
      "step": 3,
      "name": { "lt": "Sviesto ir kiaušinių plakimas" },
      "text": {
        "lt": "Dideliame dubenyje išplakite sviestą su cukrumi, kol masė taps puri. Įmuškite kiaušinius po vieną, tuomet įmaišykite vanilę."
      }
    },
    {
      "step": 4,
      "name": { "lt": "Bananų įmaišymas" },
      "text": {
        "lt": "Suberkite sutrintus bananus ir maišykite, kol masė taps vientisa."
      }
    },
    {
      "step": 5,
      "name": { "lt": "Tešlos paruošimas" },
      "text": {
        "lt": "Į sviesto ir bananų masę pakaitomis įmaišykite sausų ingredientų mišinį ir pasukas. Maišykite tik tiek, kad ingredientai susijungtų."
      }
    },
    {
      "step": 6,
      "name": { "lt": "Kepimas" },
      "text": {
        "lt": "Supilkite tešlą į formą, išlyginkite paviršių ir kepkite 45–50 min., kol įsmeigtas medinis pagaliukas išeina sausas."
      }
    },
    {
      "step": 7,
      "name": { "lt": "Atvėsinimas" },
      "text": {
        "lt": "Leiskite pyragui atvėsti formoje 10 min., tada išimkite ir dėkite ant grotelių visiškai atvėsti."
      }
    },
    {
      "step": 8,
      "name": { "lt": "Pateikimas" },
      "text": {
        "lt": "Pyragą galite patiekti su sūrio kremu, cukraus pudra arba mėgautis natūraliu skoniu."
      }
    }
  ],
