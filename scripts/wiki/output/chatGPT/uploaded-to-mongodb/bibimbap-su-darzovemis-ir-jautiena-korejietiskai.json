  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruoškite morkas"
      },
      "text": {
        "lt": "Supjaustykite morkas plonais šiaudeliais ir pavirkite sūdytame vandenyje kelias minutes. Tada nuplaukite šaltu vandeniu ir nusausinkite."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Paruoškite špinatus"
      },
      "text": {
        "lt": "Užvirinkite špinatus dideliame puode su nedideliu kiekiu vandens. Kai špinatai suminkštėja, nusausinkite ir atvėsinkite šaltu vandeniu."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Paruoškite pupelių daigus"
      },
      "text": {
        "lt": "Blanširuokite pupelių daigus sūdytame vandenyje, kol jie taps permatomi. Nusausinkite ir atvėsinkite juos."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Paruoškite jautieną"
      },
      "text": {
        "lt": "Supjaustykite jautieną plonais gabalėliais ir apkepkite karštoje keptuvėje su alyvuogių aliejumi. Pridėkite sojų padažo ir leiskite skysčiui išgaruoti."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekite patiekalą"
      },
      "text": {
        "lt": "Padėkite ryžius į dubenėlius, ant viršaus sudėkite paruoštas daržoves, jautieną ir kiaušinį. Patiekite su šonuose esančiais kimči, gochujang ir kitais ingredientais."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Sezamų sėklos padidina savo tūrį malant, todėl 2-3 šaukštai sezamų sėklų atitinka ¼ puodelio maltų sezamų."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Šis patiekalas puikiai dera su šviežių salotų dubeniu šalia."
      },
      "priority": 2
    }
