  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Orkaitės paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 175°C. Sumaišykite šokolado trupinius su cukrumi ir sviestu mažame dubenėlyje."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Pagrindo formavimas"
      },
      "text": {
        "lt": "Įspauskite mišinį į 30 cm skersmens tartos formą, kurios dugnas nuimamas. Kepkite 8-10 minučių arba kol lengvai paruduos."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Šokolado tirpinimas"
      },
      "text": {
        "lt": "Ištirpinkite sviestą ir šokoladą mikrobangų krosnelėje, maišykite, kol masė taps lygi."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Dideliame dubenyje plakite kiaušinius su cukrumi, vanile ir druska apie 4 minutes, kol masė taps puri."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Tešlos užbaigimas"
      },
      "text": {
        "lt": "Įmaišykite šokolado mišinį ir miltus. Supilkite į formą ir tolygiai paskirstykite."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kepkite 25-30 minučių arba kol įdurtas dantų krapštukas išeis sausas. Atvėsinkite ant grotelių."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Apdaila"
      },
      "text": {
        "lt": "Užtepkite vyšnių uogienę ant pyrago. Ištirpinkite šokoladą su grietinėle ir užlašinkite ant viršaus. Atšaldykite."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite naudoti ir šviežias vyšnias vietoj uogienės."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Tortas geriausiai skanus, kai jis šiek tiek atšaldytas."
      },
