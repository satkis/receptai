  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Baklažano kepimas"
      },
      "text": {
        "lt": "Kepkite baklažanus tiesiai ant žemos ugnies, kol odelė taps juoda ir trapi, o minkštimas suminkštės. Tai trunka apie 15–20 minučių."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Atvėsimas"
      },
      "text": {
        "lt": "Leiskite keptiems baklažanams atvėsti iki kambario temperatūros."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Pasta"
      },
      "text": {
        "lt": "Maisto kombaine sumalkite čili, kokosą ir kalendrą iki vientisos pastos."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Baklažano paruošimas"
      },
      "text": {
        "lt": "Nulupkite baklažanų odą ir sutrinkite minkštimą iki vientisos konsistencijos."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Maišymas"
      },
      "text": {
        "lt": "Sumaišykite baklažanų masę su kokosų pasta ir paniru. Pagardinkite druska pagal skonį."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite su chapati, dosa ar garintais ryžiais."
      }
    }
  ],
  "notes": [
