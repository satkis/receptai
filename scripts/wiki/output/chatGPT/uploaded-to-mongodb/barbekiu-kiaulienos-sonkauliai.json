  "instructions": [
    {
      "step": 1,
      "name": { "lt": "Marinatas" },
      "text": { "lt": "Padėkite šonkaulius į kepimo indą vienu sluoksniu. Sumaišykite likusius ingredientus mažame dubenėlyje ir užpilkite ant šonkaulių, gerai aptepkite. Palikite marinuotis per naktį, kartkartėmis aptepdami." }
    },
    {
      "step": 2,
      "name": { "lt": "Paruošimas kepimui" },
      "text": { "lt": "Įkaitinkite orkaitę iki 191 °C. Kepimo skardą užpildykite 1.3 cm vandens ir padėkite grotelę su šonkauliais viršuje." }
    },
    {
      "step": 3,
      "name": { "lt": "Kepimas" },
      "text": { "lt": "Kepkite šonkaulius 45 minutes." }
    },
    {
      "step": 4,
      "name": { "lt": "Temperatūros padidinimas" },
      "text": { "lt": "Pakelkite orkaitės temperatūrą iki 232 °C ir kepkite dar 15 minučių." }
    },
    {
      "step": 5,
      "name": { "lt": "Patiekimas" },
      "text": { "lt": "Patiekite karštus arba šaltus." }
    }
  ],
  "notes": [
    {
      "text": { "lt": "Alternatyviai, kepkite 191 °C orkaitėje 20 min, tada sumažinkite iki 163 °C ir kepkite 1 valandą." },
