  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Svogūnų ir česnako kepinimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų keptuvėje ir pakepinkite svogūną bei česnaką, kol suminkštės ir taps auksinės spalvos."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kiaulienos kepinimas"
      },
      "text": {
        "lt": "Į keptuvę įdėkite kiaulienos pilvą ir kepkite, kol jis taps auksinės spalvos. Retkarčiais maišykite."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Sudėkite likusius ingredientus"
      },
      "text": {
        "lt": "Į keptuvę pridėkite bagoong alamang, siling labuyo, cukrų ir juoduosius pipirus. Viską gerai išmaišykite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Troškinimas su kokosų pienu"
      },
      "text": {
        "lt": "Supilkite kokosų pieną ir leiskite troškintis ant mažos ugnies apie 30 minučių, kol skoniai susimaišys."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite karštą su ryžiais. Pagal skonį galite pridėti mononatrio glutamato."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei negalite rasti siling labuyo, galite naudoti kitų rūšių aitriąsias paprikas."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Norėdami sumažinti patiekalo aštrumą, galite sumažinti paprikų kiekį."
