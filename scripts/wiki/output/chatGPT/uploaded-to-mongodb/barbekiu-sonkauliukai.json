  "instructions": [
    {
      "step": 1,
      "name": { "lt": "Prieskonių įtrynimas" },
      "text": { "lt": "Įtrinkite šonkaulius barbekiu prieskonių mišiniu. Palikite šaldytuve 1 valandą." }
