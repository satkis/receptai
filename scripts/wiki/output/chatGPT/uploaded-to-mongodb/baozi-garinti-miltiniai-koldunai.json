  "instructions": [
    {
      "step": 1,
      "name": { "lt": "Mielių aktyvacija" },
      "text": { "lt": "Sumaišykite mieles su 1 šaukštu cukraus ir 1 puodeliu šilto vandens. Palikite 15 minučių. Jei mielės nereikalauja aktyvacijos, praleiskite šį žingsnį." }
    },
    {
      "step": 2,
      "name": { "lt": "Tešlos paruošimas" },
      "text": { "lt": "Sijokite miltus su cukrumi. Jei mielės jau sumaišytos, įdėkite jas į miltus kartu su likusiu cukrumi." }
    },
    {
      "step": 3,
      "name": { "lt": "Tešlos maišymas" },
      "text": { "lt": "Jei mielių nemaišėte, sumaišykite miltus, druską ir cukrų, tada pridėkite mieles. Įpilkite 1 puodelį vandens, nuolat maišydami." }
    },
    {
      "step": 4,
      "name": { "lt": "Tešlos minkymas" },
      "text": { "lt": "Minkykite tešlą 5–10 minučių, kol ji taps lygi ir šiek tiek blizganti." }
    },
    {
      "step": 5,
      "name": { "lt": "Tešlos kilimas" },
      "text": { "lt": "Didelį dubenį patepkite sezamo aliejumi, įdėkite tešlą ir palikite šiltoje vietoje, kol padvigubės tūris, apie 1–1,5 val." }
    },
    {
      "step": 6,
      "name": { "lt": "Formavimas" },
      "text": { "lt": "Suspauskite tešlą ir padalinkite į 12 dalių. Kiekvieną gabalėlį iškočiokite į diską, įdėkite įdarą ir užspauskite kraštus." }
    },
    {
      "step": 7,
      "name": { "lt": "Garų paruošimas" },
      "text": { "lt": "Sudėkite baozi ant pergamento popieriaus ir palikite kilti dar 1 valandą šiltoje vietoje." }
    },
    {
      "step": 8,
      "name": { "lt": "Garavimas" },
      "text": { "lt": "Garinkite baozi virš silpnos verdančio vandens srovės 10–15 minučių. Po garavimo leiskite natūraliai atvėsti." }
    }
  ],
  "notes": [
    { "text": { "lt": "Norint baltesnių bandelių, naudokite balintus miltus." }, "priority": 1 },
    { "text": { "lt": "Įdaro skonį galite keisti pagal savo pageidavimus." }, "priority": 2 }
  ],
