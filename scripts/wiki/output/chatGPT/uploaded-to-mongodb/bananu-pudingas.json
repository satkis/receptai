  "instructions": [
    {
      "step": 1,
      "name": { "lt": "Pagrindinio kremo paruošimas" },
      "text": {
        "lt": "Puode sumaišykite pieną, 200 g cukraus, sviestą, druską ir vanilę. Kaitinkite ant vidutinės ugnies, kol sviestas visiškai išsilydys ir mišinys sušils."
      }
    },
    {
      "step": 2,
      "name": { "lt": "Kiaušinių paruošimas" },
      "text": {
        "lt": "Atskirkite kiaušinių trynius. Juos išplakite su šlakeliu karšto pieno mišinio (temperavimui), kad tryniai nesutrauktų."
      }
    },
    {
      "step": 3,
      "name": { "lt": "Kremo sujungimas" },
      "text": {
        "lt": "Supilkite trynių masę į puodą ir maišykite, kol kremas taps vientisas."
      }
    },
    {
      "step": 4,
      "name": { "lt": "Kremo sutirštinimas" },
      "text": {
        "lt": "Krakmolą išmaišykite su 2 šaukštais šalto vandens ir supilkite į kremą. Kaitinkite maišydami, kol mišinys sutirštės."
      }
    },
    {
      "step": 5,
      "name": { "lt": "Meringos paruošimas" },
      "text": {
        "lt": "Kiaušinių baltymus su 25 g cukraus išplakite iki standžių, purių viršūnių."
      }
    },
    {
      "step": 6,
      "name": { "lt": "Sausainių ir bananų sluoksniavimas" },
      "text": {
        "lt": "Dubenyje išdėliokite vanilinius sausainius. Užpilkite pusę citrinos sulčių, tuomet užpilkite pusę kremo ir dėkite pusę supjaustytų bananų."
      }
    },
    {
      "step": 7,
      "name": { "lt": "Sluoksnių pabaiga" },
      "text": {
        "lt": "Pakartokite sluoksniavimą: sausainiai, likusios citrinos sultys, kremas ir bananų riekelės."
      }
    },
    {
      "step": 8,
      "name": { "lt": "Meringa ir kokosų užbaigimas" },
      "text": {
        "lt": "Užtepkite meringą ant viršaus ir pabarstykite kokosu. Šaldykite kelias valandas, kad skoniai susijungtų."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite naudoti pudingo miltelius, tačiau tokiu atveju nebeliks kiaušinių baltymų meringai."
      },
      "priority": 1
    }
  ],
