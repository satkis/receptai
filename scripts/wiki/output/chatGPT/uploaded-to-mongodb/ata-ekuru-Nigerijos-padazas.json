  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Sumaišykite raudonąsias paprikas, scotch bonnet pipirus ir kajeno pipirus. Susmulkinkite iki grubios pastos konsistencijos."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite palmių aliejų keptuvėje. Po 5 minučių įpilkite maltą pipirų mišinį į karštą aliejų. Pagardinkite druska ir prieskonių kubeliais pagal skonį, pridėkite pasirinktinus ingredientus."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Virimas"
      },
      "text": {
        "lt": "Virinkite ant vidutinės–žemos ugnies apie 30–40 minučių, kol padažas bus gerai išviręs ir aliejus pakils į paviršių."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite kartu su ekuru."
      }
    }
