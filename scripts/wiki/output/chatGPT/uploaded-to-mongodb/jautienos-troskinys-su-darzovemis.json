  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Mėsos paruošimas"
      },
      "text": {
        "lt": "Supjaustykite jautieną gabaliukais apie 2,5 cm dydžio. Sudėkite į maišelį su miltais ir gerai pakratykite."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Mėsos kepimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų plačiame puode ir apkepkite jautieną keliais etapais, kol apskrus."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Troškinio virimas"
      },
      "text": {
        "lt": "Sudėkite apkepintą jautieną, prieskonius, pomidorų sultis ir sultinį į puodą. Uždenkite ir troškinkite apie valandą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Daržovių paruošimas"
      },
      "text": {
        "lt": "Nulupkite morkas ir supjaustykite gabaliukais. Sudėkite į troškinį ir toliau virkite, kartais pamaišydami."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Daržovių pridėjimas"
      },
      "text": {
        "lt": "Supjaustykite likusias daržoves ir sudėkite į troškinį. Troškinkite, kol daržovės suminkštės."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Kai daržovės minkštos, išimkite lauro lapą ir patiekite troškinį."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galima pridėti kitų daržovių, tokių kaip ropės ar petražolės šaknys."
      },
      "priority": 1
    }
