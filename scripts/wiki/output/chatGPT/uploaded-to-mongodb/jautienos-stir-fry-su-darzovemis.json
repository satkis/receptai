  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Pasiruošimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų keptuvėje ir apkepkite svogūną su česnaku, kol česnakas taps auksinės spalvos. Išimkite ir palaikykite šiltai."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Jautienos paruošimas"
      },
      "text": {
        "lt": "Pabarstykite jautienos juosteles druska ir pipirais. Apkepkite keptuvėje tol, kol gerai apskrus. Išimkite ir pakartokite."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Daržovių kepimas"
      },
      "text": {
        "lt": "Į keptuvę įdėkite čili pipirus, morkas, sojų padažą, medų, actą ir svogūnų laiškus. Kepkite, kol daržovės lengvai apskrus."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Pabaigimas"
      },
      "text": {
        "lt": "Grąžinkite svogūną, česnaką ir jautieną į keptuvę. Gerai išmaišykite, kol viskas sušils."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Prieš patiekdami, pabarstykite skrudintomis sezamo sėklomis."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Thai bird čili pipirus galima pakeisti serrano pipirais arba trupintais raudonais pipirais."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Ribeye kepsnys yra skanus ir minkštas, tačiau galite naudoti liesesnes alternatyvas, kaip flank arba sirloin kepsnį."
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Reguliuokite sojų padažo ir medaus santykį pagal savo skonį."
      },
      "priority": 3
    }
