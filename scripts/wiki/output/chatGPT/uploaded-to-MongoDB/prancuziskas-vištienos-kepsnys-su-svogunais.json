  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Orkaitės paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 200°C. Tai padės vištienai iškepti tolygiai ir traškiai."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Vištienos paruošimas"
      },
      "text": {
        "lt": "Panardinkite vištienos krūtinėlę į plaktą kiaušinį, tada apvoliokite susmulkintuose svogūnuose. Įsitikinkite, kad vištiena yra gerai padengta."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Sudėkite vištieną ant pergamentu išklotos kepimo skardos ir kepkite orkaitėje apie 20 minučių, kol vidus nebebus rausvas."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite karštą su mėgstamais garnyrais ar salotomis. Papriką galite pridėti pagal skonį."
      }
    }
  ],
