  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Orkaitės paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 180°C."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Ingredientų maišymas"
      },
      "text": {
        "lt": "Dubenyje sumaišykite cukrų ir kokoso drožles. Kitame dubenyje suplakite kiaušinį."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos formavimas"
      },
      "text": {
        "lt": "Į sausus ingredientus įmaišykite kiaušinį (ir maisto dažiklius, jei naudojate). Gerai išmaišykite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Pyragėlių formavimas"
      },
      "text": {
        "lt": "Rankomis arba naudodami kiaušinio puodelį suformuokite 6-7 piramides (pyragėlius) ir išdėliokite ant kepimo popieriumi išklotos skardos."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Vyšnių pridėjimas"
      },
      "text": {
        "lt": "Įspauskite vyšnią į kiekvieno pyragėlio viršūnę, jei naudojate."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kepkite 15-20 minučių, kol piramidės taps šviesiai auksinės spalvos."
      }
    }
  ],
  "notes": [
    {
