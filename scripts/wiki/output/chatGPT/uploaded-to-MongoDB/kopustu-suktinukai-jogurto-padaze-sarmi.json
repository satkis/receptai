  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Lapų paruošimas"
      },
      "text": {
        "lt": "Užvirinkite vynuogių arba kopūstų lapus sūriame vandenyje, kol jie suminkštės. Nukoškite ir leiskite atvėsti."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Įdaro paruošimas"
      },
      "text": {
        "lt": "Smulkiai supjaustytus svogūnų laiškus pakepinkite aliejuje. Įdėkite paprikos ir išmaišykite."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Mėsos ir ryžių maišymas"
      },
      "text": {
        "lt": "Įdėkite mėsą, ryžius, pipirus, mėtas ir smulkintas petražoles į svogūnus. Kepkite, kol mėsos spalva pasikeis."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Ryžių virimas"
      },
      "text": {
        "lt": "Įpilkite šiek tiek daugiau, nei pusę puodelio vandens, užvirinkite ir tada sumažinkite ugnį. Troškinkite, kol ryžiai sugeria vandenį. Leiskite atvėsti."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Suktinukų formavimas"
      },
      "text": {
        "lt": "Ant kiekvieno lapo dėkite po šaukštelį įdaro, suvyniokite į suktinuką."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Suktinukų troškinimas"
      },
      "text": {
        "lt": "Suktinukus sudėkite į puodą, užpilkite vandeniu ir troškinkite."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Padažo paruošimas"
      },
      "text": {
        "lt": "Jogurtą sumaišykite su paprika ir kitais prieskoniais."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Atvėsusius suktinukus perkelkite į lėkštę ir užpilkite padažu."
      }
    }
  ],
