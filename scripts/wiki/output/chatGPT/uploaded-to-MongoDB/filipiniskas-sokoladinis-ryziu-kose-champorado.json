  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Ryžių paruošimas"
      },
      "text": {
        "lt": "Du kartus praplaukite ryžius po tekančiu vandeniu. Tai padės pašalinti perteklinį krakmolą."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Virimui"
      },
      "text": {
        "lt": "Puode sumaišykite ryžius ir vandenį. Uždenkite ir virkite ant vidutinės ugnies, kol užvirs."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Maišymas"
      },
      "text": {
        "lt": "Atidenkite puodą ir įmaišykite cukrų bei kakavos miltelius. Gerai išmaišykite, kad neliktų gumulų."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Troškinimas"
      },
      "text": {
        "lt": "Sumažinkite ugnį ir troškinkite košę, retkarčiais maišydami, kol ryžiai suminkštės ir košė taps tiršta."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Įmaišykite kondensuotą pieną ir patiekite karštą. Skanaus!"
      }
    }
  ],
  "notes": [
