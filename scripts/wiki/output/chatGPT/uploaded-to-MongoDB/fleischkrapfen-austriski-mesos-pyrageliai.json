  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Sumaišykite ruginių ir kviečių miltus, įberkite druskos. Užvirkite pieną su sviestu ir supilkite į miltus. Greitai užminkykite tešlą."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Įdaro paruošimas"
      },
      "text": {
        "lt": "Smulkiai supjaustykite svogūną ir apkepkite svieste. Sudėkite šoninę, rūkytą mėsą ir virtas, smulkintas bulves. Pagardinkite druska, pipirais, mažaramu ir petražolėmis. Leiskite šiek tiek atvėsti."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Pyragėlių formavimas"
      },
      "text": {
        "lt": "Iškočiokite tešlą į 4 cm storio gabalą. Supjaustykite į 2 cm storio griežinėlius ir iškočiokite į apskritimus. Įdėkite įdarą ant vienos pusės, uždenkite kita puse ir tvirtai suspauskite kraštus."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų iki 180°C. Kepkite pyragėlius iki auksinės spalvos, kol bus traškūs."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite su raugintais kopūstais šalia. Skanaus!"
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei neturite mažaramo, galite naudoti čiobrelius."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Šie pyragėliai puikiai tinka tiek karšti, tiek atšalę."
