  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Bulvių paruošimas"
      },
      "text": {
        "lt": "Nulupkite bulves ir supjaustykite jas skiltelėmis. Jos gali būti mažos arba didelės, kaip jums patinka."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Bulvių kepimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų keptuvėje iki maždaug 175°C. Kepkite bulves aliejuje, kol jos taps šviesiai rudos. Kai bulvės iškeps, pasūdykite jas pagal skonį."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Vištienos kepimas"
      },
      "text": {
        "lt": "Kepkite vištieną apie 5 minutes. Išleiskite aliejų arba perkelkite vištieną į kitą keptuvę."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Vištienos troškinimas"
      },
      "text": {
        "lt": "Pabarstykite vištieną druska, įpilkite 180 ml vandens ir troškinkite 15–20 minučių."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Galutinis paruošimas"
      },
      "text": {
        "lt": "Sumaišykite vištieną ir bulves (bulvės viršuje). Įdėkite kumino ir troškinkite dar apie 20 minučių."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei norite, kad bulvės būtų traškesnės, galite jas šiek tiek paspausti prieš kepdami."
      },
      "priority": 1
    }
