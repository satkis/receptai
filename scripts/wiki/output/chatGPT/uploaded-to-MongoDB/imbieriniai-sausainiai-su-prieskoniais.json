  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Sviesto ir prieskonių maišymas"
      },
      "text": {
        "lt": "Sumaišykite tirpintą sviestą, melasą, cukrų ir prieskonius dideliame dubenyje. Tai bus jūsų sausainių bazė."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Įmaišykite miltus, po vieną puodelį vienu metu, iki 10 puodelių, naudodami šakutę, o paskui ir rankomis. Jei tešla vis dar lipni, pridėkite dar 1–2 puodelius miltų, kol tešla taps vientisa."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos atvėsinimas"
      },
      "text": {
        "lt": "Tešlą padėkite į šaldytuvą bent 2 valandoms, kad ji sutvirtėtų. Tai padės sausainiams išlaikyti formą kepant."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Orkaitės paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 175°C. Tai pasiruošimas kepimui."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Tešlos formavimas"
      },
      "text": {
        "lt": "Iškočiokite tešlą kuo ploniau ir išpjaukite formeles su sausainių formelėmis. Tai užtikrins gražius sausainius."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Sausainių formeles perkelkite ant kepimo popieriumi išklotos skardos ir kepkite 7 minutes. Leiskite sausainiams atvėsti ant grotelių."
      }
    }
  ],
  "notes": [
    {
