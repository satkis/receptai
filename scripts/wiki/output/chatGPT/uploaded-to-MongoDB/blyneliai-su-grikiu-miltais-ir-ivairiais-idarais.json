  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Sumaišykite grikių miltus ir druską. Įpilkite kiaušinį ir pusę vandens, plakite iki vientisos masės."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos poilsis"
      },
      "text": {
        "lt": "Uždenkite tešlą audiniu ir palikite valandai pastovėti kambario temperatūroje."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Vandens pridėjimas"
      },
      "text": {
        "lt": "Poilsio metu palaipsniui įmaišykite likusį vandenį, kad tešla būtų tinkamos konsistencijos."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Blynų kepimas"
      },
      "text": {
        "lt": "Įkaitinkite didelę keptuvę ir sutepkite ją sviestu. Įpilkite tešlos, kad blynelis būtų ne storesnis nei 3 mm. Kepkite, kol viršus išdžius, ir apverskite."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Įdarų pridėjimas"
      },
      "text": {
        "lt": "Jei norite įdaryti, dėkite įdarus į centrą ir sulankstykite blyną, kad susidarytų kvadratas."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite naudoti įvairius įdarus, tokius kaip sūris, mėsa ar daržovės."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Įdėjus įdarą, blynus galima trumpam pakaitinti keptuvėje."
