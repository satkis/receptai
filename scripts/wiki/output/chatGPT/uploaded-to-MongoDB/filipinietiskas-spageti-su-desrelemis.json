  "sideIngredients": [],
  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Aliejaus kaitinimas"
      },
      "text": {
        "lt": "Įkaitinkite augalinį aliejų keptuvėje ant vidutinės ugnies."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Dešrelių kepimas"
      },
      "text": {
        "lt": "Įdėkite dešreles ir kepkite, dažnai maišydami, apie 3–4 minutes, kol apskrus."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Svogūnų ir česnakų kepimas"
      },
      "text": {
        "lt": "Išimkite dešreles, palikdami aliejų, ir sudėkite svogūnus bei druską. Kepkite 5–6 minutes, kol suminkštės. Tada pridėkite česnakus ir kepkite dar minutę."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Maltos kiaulienos kepimas"
      },
      "text": {
        "lt": "Įdėkite maltą kiaulieną ir kepkite, maišydami, kol ji taps vienodai iškepusi."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Padažo paruošimas"
      },
      "text": {
        "lt": "Įdėkite pomidorų pastą ir kepkite minutę. Grąžinkite dešreles į keptuvę, pridėkite bananų kečupą, pomidorų tyrę, vištienos sultinį ir sojų padažą. Gerai išmaišykite."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Padažo troškinimas"
      },
      "text": {
        "lt": "Užvirkite, tada sumažinkite ugnį ir troškinkite 10–15 minučių, kad skoniai susimaišytų."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Spagečių virimas"
      },
      "text": {
        "lt": "Kol padažas troškinasi, virkite spagečius pagal pakuotės nurodymus iki al dente. Nusausinkite ir grąžinkite į puodą."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Įmaišykite kelis šaukštus padažo į spagečius, kad jie būtų padengti. Patiekite lėkštėse, užpilkite likusį padažą ir pabarstykite tarkuotu čedario sūriu."
      }
    }
  ],
