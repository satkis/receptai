  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Aliejaus kaitinimas"
      },
      "text": {
        "lt": "Įkaitink aliejų didelėje keptuvėje ant vidutinės ugnies."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Svogūnų kepimas"
      },
      "text": {
        "lt": "Suberkite pjaustytus svogūnus ir kepkite 2–3 minutes, kol jie suminkštės."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Prieskonių maišymas"
      },
      "text": {
        "lt": "Įmaišykite kario miltelius, čili miltelius ir smulkintas paprikas. Kepkite dar 1 minutę, kol pradės sklisti aromatas."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Daržovių troškinimas"
      },
      "text": {
        "lt": "Sudėkite pjaustytus pomidorus, tarkuotas morkas ir paprikas. Gerai išmaišykite."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Troškinio užbaigimas"
      },
      "text": {
        "lt": "Įpilkite sultinio kubelį ir vandenį. Uždenkite ir troškinkite 10–15 minučių, kol daržovės taps minkštos. Paragaukite ir, jei reikia, pagardinkite druska ir pipirais."
      }
    }
  ],
