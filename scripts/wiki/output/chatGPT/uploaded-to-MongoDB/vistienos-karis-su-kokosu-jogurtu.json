  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Marinavimas"
      },
      "text": {
        "lt": "Nuvalykite vištieną ir supjaustykite mažais gabalėliais. Sumaišykite su visais marinato ingredientais ir leiskite pastovėti 30 minučių, kad skoniai įsigertų."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Prieskonių kepimas"
      },
      "text": {
        "lt": "Didelėje keptuvėje ant vidutinės ugnies apkepkite kumino, pankolio bei garstyčių sėklas."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Česnako ir imbiero kepimas"
      },
      "text": {
        "lt": "Į keptuvę įdėkite česnako ir imbiero pastą ir kepkite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Svogūnų kepimas"
      },
      "text": {
        "lt": "Svogūną supjaustykite mažais gabalėliais ir suberkite į keptuvę."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Bulvių įdėjimas"
      },
      "text": {
        "lt": "Supjaustykite bulves smulkesniais gabalėliais. Suberkite į keptuvę ir apkepkite su visais prieskoniais."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Vištienos apkepimas"
      },
      "text": {
        "lt": "Iškepus bulvėms suberkite į atskirą indą. Suberkite į keptuvę vištienos gabalėlius ir iškepkite kol šiek tiek apskrus."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Patiekalo užbaigimas"
      },
      "text": {
        "lt": "Įdėkite kario miltelių, kokosų jogurto ir sumažinkite ugnį. Suberkite bulves kartu su vištiena. Leiskite viskam troškintis 35–40 minučių."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Pateikimas"
      },
      "text": {
        "lt": "Smulkiai supjaustykite šviežią kalendrą ir pridėkite citrinos sulčių. Patiekite su ryžiais arba naan duona."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei neturite kokosų jogurto, galite naudoti įprastą jogurtą."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Kario skonis bus intensyvesnis, jei leisite jam pastovėti vieną naktį."
      },
      "priority": 2
    }
