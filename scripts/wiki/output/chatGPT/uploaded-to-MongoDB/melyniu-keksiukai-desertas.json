  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Patepkite keksiukų formeles aliejumi arba išklokite popierinėmis formelėmis; atidėkite į šalį."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Orkaitės paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 200°C."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Mišinio ruošimas"
      },
      "text": {
        "lt": "Vidutinio dydžio dubenyje sumaišykite miltus, cukrų, kepimo miltelius ir druską. Padarykite duobutę mišinio viduryje ir atidėkite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Skystų ingredientų sumaišymas"
      },
      "text": {
        "lt": "Mažame dubenyje sumaišykite plakta kiaušinį, pieną ir aliejų. Supilkite šį mišinį į miltų mišinį ir įmaišykite mėlynes. Maišykite tik tol, kol ingredientai susimaišys. Nepersistenkite."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Keksiukų formavimas"
      },
      "text": {
        "lt": "Tešlą sudėkite į paruoštas formeles, užpildydami apie du trečdalius jų talpos."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kepkite 18-20 minučių arba kol keksiukai taps auksinės spalvos. Įkišus medinį dantų krapštuką į keksiuko vidurį jis turėtų išeiti švarus."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Atvėsinimas"
      },
      "text": {
        "lt": "Išimkite keksiukus iš formelių ir leiskite jiems atvėsti ant vielinės lentynos apie 5 minutes."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Mišinys po maišymo turi likti su gumulėliais. Per daug maišant keksiukai gali būti kieti."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Jei paliksite keksiukus formelėse, jie gali permirkti."
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Norėdami pagaminti didesnius keksiukus, sumažinkite temperatūrą iki 180°C ir kepkite 30 minučių."
      },
      "priority": 3
    }
  ],
