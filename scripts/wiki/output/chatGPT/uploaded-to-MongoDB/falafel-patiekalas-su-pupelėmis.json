  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Avinžirnių paruošimas"
      },
      "text": {
        "lt": "Iš vakaro avinžirnius užmerkite vandenyje, kad jie išbrinktų. Prieš gaminant, nusausinkite ir gerai nuplaukite."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Ingredientų maišymas"
      },
      "text": {
        "lt": "Avinžirnius sumalkite smulkintuvu iki tyrimo konsistencijos. Įdėkite česnaką, petražoles, svogūną, kalendrą, kmynus, kajeno pipirus, kepimo miltelius ir miltus. Viską gerai sumaišykite."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Formavimas"
      },
      "text": {
        "lt": "Iš paruoštos masės formuokite mažus rutuliukus, naudodami maždaug šaukštą mišinio kiekvienam."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų keptuvėje. Į jį dėkite rutuliukus ir kepkite, kol gražiai apskrus iš visų pusių."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Falafelius patiekite su pita duona arba mėgstamais pagardais. Skanaus!"
      }
    }
  ],
