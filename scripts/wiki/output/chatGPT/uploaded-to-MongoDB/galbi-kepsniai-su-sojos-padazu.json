  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Marinavimas"
      },
      "text": {
        "lt": "Sumaišykite sojos padažą, cukrų, smulkintą česnaką, sezamo aliejų ir smulkintą svogūną dubenyje. Įdėkite šonkaulius ir gerai juos aptepkite marinatu. Palikite marinuotis šaltai bent 2 valandas arba per naktį."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite grilį ar keptuvę. Kepkite šonkaulius ant vidutinės ugnies, kol jie bus gerai apskrudę ir iškepę, maždaug 5-7 minutes iš kiekvienos pusės."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite galbi karštus, apibarstykite juodaisiais pipirais pagal skonį. Puikiai tinka su ryžiais ar šviežiomis daržovėmis."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei norite intensyvesnio skonio, marinuokite šonkaulius per naktį."
      },
