  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite keptuvę ant vidutinės ugnies ir ištirpinkite sviestą."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įdėkite bananų griežinėlius į keptuvę ir apibarstykite juos rudu cukrumi."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Karamelizavimas"
      },
      "text": {
        "lt": "Kepkite bananus 2–3 minutes iš abiejų pusių, kol taps auksiniai ir karamelizuoti."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Papildymas"
      },
      "text": {
        "lt": "Jei norite, kepimo metu pabarstykite cinamonu."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Išimkite keptus bananus iš keptuvės ir patiekite karštus. Galite papildyti vaniliniais ledais, jei norite."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Eksperimentuokite su skirtingais prieskoniais, tokiais kaip muskato riešutas, kad suteiktumėte dar unikalesnį skonį."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Papildomam saldumui apšlakstykite medumi arba šokoladiniu sirupu."
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Keptus bananus galima naudoti kaip priedą blynams, vafliams ar prancūziškam skrebučiui."
      },
      "priority": 3
    }
  ],
