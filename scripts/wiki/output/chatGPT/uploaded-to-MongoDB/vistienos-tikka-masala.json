  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Aliejaus kaitinimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų didelėje keptuvėje ar wok'e."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Česnakų ir svogūnų kepimas"
      },
      "text": {
        "lt": "Įpilkite česnakus ir svogūnus, ir kepkite, kol jie paruduos."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Karijaus pastos maišymas"
      },
      "text": {
        "lt": "Įmaišykite karijaus pastas ir gerai sumaišykite, pridedant šiek tiek vandens ar jogurto."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Vištienos pridėjimas"
      },
      "text": {
        "lt": "Įdėkite keptą vištienos tikka ir kepkite dar dvi minutes, kad įkaistų."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Pomidorų ir prieskonių pridėjimas"
      },
      "text": {
        "lt": "Pridėkite pomidorus, actą, pomidorų pastą ir paprikas, ir troškinkite penkias minutes. Jei reikia, įpilkite daugiau vandens."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Grietinėlės ir prieskonių pridėjimas"
      },
      "text": {
        "lt": "Įpilkite grietinėlę, garam masala ir smulkintas kalendras, ir dar šiek tiek patroškinkite."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Papuoškite šviežiomis kalendromis ir patiekite su Naan duona ar ryžiais."
      }
    }
  ],
