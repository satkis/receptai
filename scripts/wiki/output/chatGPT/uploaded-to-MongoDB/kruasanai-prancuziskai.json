  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Sumaišykite šiltą pieną su sausomis mielėmis ir cukrumi. Palikite apie 30 minučių, kol mielės suputos."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos maišymas"
      },
      "text": {
        "lt": "Dideliame dubenyje sumaišykite miltus ir druską. Palaipsniui įpilkite mielių mišinį ir maišykite, kol susidarys tešla."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Sviesto paruošimas"
      },
      "text": {
        "lt": "Sviestą tarp dviejų kepimo popieriaus lapų suplokite į ploną lakštą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos laminavimas"
      },
      "text": {
        "lt": "Ant miltuoto paviršiaus iškočiokite tešlą į didelį stačiakampį. Uždenkite pusę tešlos sviesto lakštu ir perlenkite per pusę."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Tešlos sluoksniavimas"
      },
      "text": {
        "lt": "Tešlą vėl iškočiokite ir sulenkite į tris dalis. Kartokite šį procesą 3–4 kartus, tarp kiekvieno lenkimo tešlą atvėsindami."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Kruasanų formavimas"
      },
      "text": {
        "lt": "Iškočiokite tešlą į didelį stačiakampį ir supjaustykite trikampiais. Kiekvieną trikampį suvyniokite nuo bazės iki viršūnės, suformuodami kruasaną."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Kildinimas"
      },
      "text": {
        "lt": "Paruoštus kruasanus sudėkite ant kepimo skardos ir leiskite jiems pakilti apie 1–2 valandas, kol išsipūs."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 200°C. Išplakite kiaušinį ir aptepkite juo kruasanus. Kepkite 15–20 minučių, kol gražiai paruduos."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite naudoti šokolado gabalėlius, kad įdarytumėte kruasanus."
      },
      "priority": 1
    }
  ],
