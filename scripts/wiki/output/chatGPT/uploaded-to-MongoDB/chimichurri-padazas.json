  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Sudedamųjų dalių paruošimas"
      },
      "text": {
        "lt": "Sumaišykite visus ingredientus (žoleles susmulkinkite), išskyrus aliejų, actą ir vandenį, dideliame dubenyje. Gerai išmaišykite, kad visos žolelės ir prieskoniai būtų tolygiai pasiskirstę."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Poilsio laikas"
      },
      "text": {
        "lt": "Palikite mišinį pailsėti 30 minučių."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Skonių sujungimas"
      },
      "text": {
        "lt": "Užvirinkite actą ir vandenį, ir užpilkite ant kitų ingredientų. Gerai išmaišykite, kad svogūnų ir česnakų skonis sušvelnėtų."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Dar kartą pailsėti"
      },
      "text": {
        "lt": "Palikite mišinį pailsėti dar 30 minučių."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Aliejaus pridėjimas"
      },
      "text": {
        "lt": "Įpilkite aliejų ir gerai išmaišykite. Jei skystis neuždenka ingredientų, pridėkite aliejaus, vandens ir acto santykiu 2:1:1."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Brandinimas"
      },
      "text": {
        "lt": "Perpilkite į hermetišką indą ir laikykite šaldytuve bent per naktį."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Kai reikia, išimkite iš šaldytuvo apie 30 minučių prieš patiekiant, kad aliejus vėl suskystėtų. Gerai išmaišykite ir patiekite."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Raudonam chimichurri naudokite 50 ml raudonojo vyno acto ir pridėkite 1 pomidorą bei ½ raudonos paprikos."
