  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Orkaitės įkaitinimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 195°C, jei norite minkštesnių sausainių. Jei mėgstate traškesnius, įkaitinkite iki 210°C."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Sviesto sumaišymas"
      },
      "text": {
        "lt": "Didelėje dubenyje sumaišykite sviestą, baltąjį ir rudąjį cukrų, kol masė taps vientisa."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Kiaušinių ir vanilės pridėjimas"
      },
      "text": {
        "lt": "Pridėkite kiaušinius po vieną, gerai išplakdami po kiekvieno. Įmaišykite vanilės ekstraktą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Sausų ingredientų įmaišymas"
      },
      "text": {
        "lt": "Atskiram dubenyje sumaišykite miltus, sodą ir druską. Įmaišykite sausus ingredientus į sviesto ir cukraus mišinį dviem dalimis."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Šokolado ir riešutų pridėjimas"
      },
      "text": {
        "lt": "Įmaišykite šokoladinius lašelius ir riešutus, jei naudojate."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Šaukšteliais formuokite tešlos kamuoliukus ir dėkite ant kepimo popieriumi išklotos skardos. Kepkite 10–12 minučių arba kol sausainiai paruduos. Leiskite jiems atvėsti ant grotelių."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei norite sausainių su minkštu viduriu, ištraukite juos iš orkaitės minutę anksčiau."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Norėdami intensyvesnio riešutų skonio, prieš dedant į tešlą, riešutus švelniai paskrudinkite."
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Didesnis vanilės ekstrakto kiekis suteiks sausainiams ryškesnį skonį."
      },
      "priority": 3
    }
  ],
