  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Pasiruošimas"
      },
      "text": {
        "lt": "Jei naudojate tofu, jį apkepkite. Jei naudojate svogūnus, supjaustykite juos norimo dydžio gabaliukais."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Daržovių paruošimas"
      },
      "text": {
        "lt": "Jei naudojate bok choy, atskirkite lapus nuo kotelių. Supjaustykite lapus skersai į juostas."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Mėsos paruošimas"
      },
      "text": {
        "lt": "Jei naudojate mėsą, supjaustykite ją gabaliukais ir pakepinkite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Keptuvės paruošimas"
      },
      "text": {
        "lt": "Į didelę keptuvę įpilkite daug aliejaus, kad ryžiai nepriliptų. Įkaitinkite ant vidutinės-aukštos ugnies."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Ryžių kepimas"
      },
      "text": {
        "lt": "Į keptuvę sudėkite ryžius. Virkite maišydami, kol jie bus karšti ir šiek tiek apskrudę."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Ingredientų pridėjimas"
      },
      "text": {
        "lt": "Įpilkite sojos padažo, pridėkite mėsą, kiaušinius ir visas daržoves, išskyrus svogūnus ir pupelių daigus."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Pagardinimas"
      },
      "text": {
        "lt": "Pagardinkite pipirais, cukrumi ir, jei norite, druska."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Baigiamieji darbai"
      },
      "text": {
        "lt": "Kai ryžiai beveik paruošti, pridėkite tofu, pupelių daigus ir svogūnus. Būkite atsargūs, kad neperkeptumėte."
      }
    }
  ],
