  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Aliejaus kaitinimas"
      },
      "text": {
        "lt": "Įkaitinkite alyvuogių aliejų dideliame puode ant vidutinės ugnies."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Daržovių kepinimas"
      },
      "text": {
        "lt": "Supjausčius smulkiai į puodą sudėkit pankolį, svogūną, šalotus ir druską. Kepkite, kol svogūnai taps permatomi."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Prieskonių pridėjimas"
      },
      "text": {
        "lt": "Įdėkite česnakus ir raudonųjų pipirų dribsnius. Kepkite dar 2 minutes."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Pomidorų pastos įmaišymas"
      },
      "text": {
        "lt": "Įmaišykite pomidorų pastą."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Skysčių pridėjimas"
      },
      "text": {
        "lt": "Įpilkite pomidorus su sultimis, vyną, vandenį, krabus, salierus ir lauro lapą. Uždenkite ir užvirkite."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Troškinimas"
      },
      "text": {
        "lt": "Sumažinkite ugnį iki vidutinės-žemos. Troškinkite uždengtą apie 30 minučių, kad skoniai susijungtų."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Midijų pridėjimas"
      },
      "text": {
        "lt": "Įdėkite midijas į troškinį. Uždenkite ir virkite, kol midijos pradės atsidaryti, apie 5 minutes."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Skalopų ir žuvies pridėjimas"
      },
      "text": {
        "lt": "Įdėkite skalopus ir žuvį. Troškinkite, kol žuvys ir skalopai bus išvirę ir midijos pilnai atsidarę, švelniai maišydami. Tai užtruks dar apie 10 minučių."
      }
    },
    {
      "step": 9,
      "name": {
        "lt": "Pabaiga"
      },
      "text": {
        "lt": "Patikrinkite, jei yra neatidarytų midijų - išmeskite jas. Pašalinkite lauro lapą."
      }
    },
    {
      "step": 10,
      "name": {
        "lt": "Pagardinimas"
      },
      "text": {
        "lt": "Pagardinkite troškinį druska ir raudonųjų pipirų dribsniais pagal skonį."
      }
    }
  ],
