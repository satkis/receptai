  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Vandens virimas"
      },
      "text": {
        "lt": "Užvirinkite vandenį puode."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Arbatos paruošimas"
      },
      "text": {
        "lt": "Įdėkite 2 arbat. šaukštelius džiovintos žaliosios arbatos į arbatinuką."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Arbatos plovimas"
      },
      "text": {
        "lt": "Supilkite pusę stiklinės verdančio vandens į arbatinuką, pamaišykite ir išpilkite vandenį, kad nuplautumėte kartų poskonį."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Mėtų ir cukraus pridėjimas"
      },
      "text": {
        "lt": "Užpildykite arbatinuką verdančiu vandeniu, pridėkite šviežius mėtų lapus ir cukraus kubelius, palaikykite ant mažos ugnies keletą minučių."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Arbatos užbaigimas"
      },
      "text": {
        "lt": "Arbata paruošta, kai vanduo užverda. Patiekite nedidelėse stiklinėse."
      }
    }
  ],
