  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Morkų paruošimas"
      },
      "text": {
        "lt": "Nupjaukite morkų galus, nuplaukite jas ir nulupkite. Tada sutarkuokite ir sudėkite į puodą."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Ingredientų sumaišymas"
      },
      "text": {
        "lt": "Pridėkite anakardžius, razinas, druską, cukrų, kardamoną, cinamoną, sviestą ir pieną į puodą su morkomis."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Virimas"
      },
      "text": {
        "lt": "Užvirkite puodą ant vidutinės ugnies, nuolat maišydami. Virkite, kol pienas beveik išgaruos."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite su cinamono milteliais ant viršaus. Mėgaukitės šiuo puikiu desertu!"
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei norite sodresnio skonio, galite pridėti 50–100g khoya beveik išgaravus pienui."
