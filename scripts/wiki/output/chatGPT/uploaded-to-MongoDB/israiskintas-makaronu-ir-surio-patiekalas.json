  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Įkaitinimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 180°C ir ištepkite kepimo indą riebalais."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Makaronų virimas"
      },
      "text": {
        "lt": "Dideliame puode užvirinkite 2 litrus pasūdyto vandens. Sudėkite makaronus ir virkite 9 minutes arba kol jie taps al dente."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Padažo ruošimas"
      },
      "text": {
        "lt": "Kol makaronai verda, pasiruoškite padažą. Puode sumaišykite miltus su sviestu ir kaitinkite ant mažos ugnies, maišydami, kol mišinys taps vienalytis ir burbuliuojantis."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Sūrio padažas"
      },
      "text": {
        "lt": "Į miltų ir sviesto mišinį įpilkite pieno, druskos ir pipirų. Maišykite, kol sutirštės. Sumažinkite ugnį iki minimalios ir įdėkite sūrį, dešreles ir šaldytas daržoves."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimo paruošimas"
      },
      "text": {
        "lt": "Kai makaronai išvirs, nupilkite vandenį ir sudėkite į paruoštą kepimo indą. Užpilkite padažą ant makaronų ir viską gerai išmaišykite."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Papuošimas ir kepimas"
      },
      "text": {
        "lt": "Ant viršaus sudėkite pomidorų griežinėlius, pabarstykite parmezanu ir paprika. Kepkite 40 minučių."
      }
    }
  ],
