  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Sviesto ir cukraus tirpinimas"
      },
      "text": {
        "lt": "Puode švelniai kaitink sviestą, cukrų ir auksinį sirupą, kol sviestas ištirps."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos maišymas"
      },
      "text": {
        "lt": "Į dubenį sudėk sausas sudedamąsias dalis. Supilk lydytą sviesto mišinį ir gerai išplak."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Pieno pašildymas"
      },
      "text": {
        "lt": "Puode lengvai pašildyk pieną, kad jis ištirpdytų sirupą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Pieno įmaišymas"
      },
      "text": {
        "lt": "Įpilk pašildytą pieną į dubenį ir gerai išmaišyk."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kiaušinių įmaišymas"
      },
      "text": {
        "lt": "Išplak kiaušinius ir įmaišyk juos į tešlą."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Paruošimas kepimui"
      },
      "text": {
        "lt": "Supilk tešlą į 20 cm išteptą kepimo formą."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kepk 170°C temperatūroje 1 valandą."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Atvėsinimas"
      },
      "text": {
        "lt": "Atvėsink pyragą formoje 15 minučių, tada išimk."
      }
    },
    {
      "step": 9,
      "name": {
        "lt": "Laikymas ir patiekimas"
      },
      "text": {
        "lt": "Palik pyragą bent 1 dieną prieš pjaustymą kvadratėliais. Patiek."
      }
    }
  ],
