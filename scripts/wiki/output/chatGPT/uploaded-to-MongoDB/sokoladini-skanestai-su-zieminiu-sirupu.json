  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Išpurškite kepimo skardą aliejumi arba lengvai patepkite sviestu. Atidėkite į šalį."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Mišinio paruošimas"
      },
      "text": {
        "lt": "Supilkite kukurūzų sirupą ir cukrų į puodą. Kaitinkite ant vidutinės ugnies, kol cukrus ištirps ir pradės burbuliuoti. Nuimkite nuo ugnies, kad nesukietėtų skanėstai."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Pridėkite riešutų sviestą"
      },
      "text": {
        "lt": "Įdėkite žemės riešutų sviestą ir maišykite, kol viskas gerai susimaišys."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Įmaišykite dribsnius"
      },
      "text": {
        "lt": "Įmaišykite pūstų ryžių dribsnius. Geriau maišykite po pusę, kad būtų lengviau."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Formavimas"
      },
      "text": {
        "lt": "Visą mišinį perkelkite į kepimo skardą ir švelniai suspauskite į lygų sluoksnį. Neprispauskite per stipriai, kad skanėstai nebūtų kieti."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Šokolado užtepimas"
      },
      "text": {
        "lt": "Puode ištirpinkite šokolado ir karamelės gabalėlius. Kai jie ištirps, paskleiskite mišinį ant dribsnių sluoksnio."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Atvėsinimas ir patiekimas"
      },
      "text": {
        "lt": "Leiskite sustingti šaldytuve apie valandą. Supjaustykite į mažus gabalėlius ir patiekite kambario temperatūroje."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Laikykite kambario temperatūroje sandariai uždengtus. Gali būti laikomi iki savaitės."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Galite sumažinti cukraus kiekį per pusę, jei norite minkštesnių skanėstų."
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Šokolado ir karamelės gabalėlius galima ištirpinti mikrobangų krosnelėje."
      },
      "priority": 3
    }
  ],
