  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Persijokite miltus į maišymo indą. Įmaišykite mieles, druską ir kepimo miltelius."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos ruošimas"
      },
      "text": {
        "lt": "Įpilkite vandens ir sumaišykite, kol gausite vientisą tešlą. Minkykite, kol tešla taps elastinga ir gerai sumaišyta."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos kildinimas"
      },
      "text": {
        "lt": "Uždenkite tešlą ir leiskite jai pakilti 1 valandą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Formavimas"
      },
      "text": {
        "lt": "Švelniai išmuškite orą iš tešlos. Sutepkite kepimo formą ir perkelkite tešlą į formą."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Virimas"
      },
      "text": {
        "lt": "Uždenkite formą ir įdėkite į garų puodą. Virkite kol duona bus iškepusi ir pagaliukas, įkištas į centrą, išeis švarus."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Išimkite iš garų puodo ir leiskite atvėsti. Išimkite iš formos ir patiekite."
      }
    }
  ],
