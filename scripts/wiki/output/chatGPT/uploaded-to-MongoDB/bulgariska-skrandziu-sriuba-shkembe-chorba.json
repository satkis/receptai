  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Dideliame puode sumaišykite svogūną, papriką ir sviestą. Kepkite, kol daržovės suminkštės."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Padažo gaminimas"
      },
      "text": {
        "lt": "Pabarstykite miltus ant svogūnų mišinio, tada įmaišykite pomidorų pastą."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Skrandžių paruošimas"
      },
      "text": {
        "lt": "Skrandžius supjaustykite plonais gabalėliais ir pridėkite prie svogūnų mišinio."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Sriubos virimas"
      },
      "text": {
        "lt": "Supilkite sultinį, pridėkite druską, mairūną ir lauro lapą. Uždenkite ir troškinkite apie 30 minučių."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Išimkite ir išmeskite lauro lapą. Supilkite sriubą į dubenėlius. Maišykite petražoles, česnaką ir sūrį. Pabarstykite ant karštos sriubos ir iškart patiekite."
      }
    }
  ],
