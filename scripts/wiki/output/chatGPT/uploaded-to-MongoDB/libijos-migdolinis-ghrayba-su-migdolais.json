  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Orkaitės paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 175°C ir paruoškite kepimo skardą su kepimo popieriumi arba silikoniniu kilimėliu."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Sviesto ir cukraus maišymas"
      },
      "text": {
        "lt": "Dubenyje sumaišykite sviestą ir cukraus pudrą, kol mišinys taps šviesus ir purus."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos ruošimas"
      },
      "text": {
        "lt": "Į sviesto mišinį įdėkite miltus, maltus migdolus ir vanilės ekstraktą. Maišykite, kol gausite minkštą tešlą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Sausainių formavimas"
      },
      "text": {
        "lt": "Nuimkite mažus tešlos gabalėlius ir suformuokite juos į riešuto dydžio rutuliukus. Padėkite rutuliukus ant paruoštos kepimo skardos, palikdami tarp jų šiek tiek vietos."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Dekoravimas ir kepimas"
      },
      "text": {
        "lt": "Į kiekvieno sausainio centrą įspauskite migdolą ir šiek tiek suplokite. Kepkite orkaitėje apie 12-15 minučių, kol sausainių kraštai taps auksiniai."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Atvėsinimas"
      },
      "text": {
        "lt": "Išimkite iš orkaitės ir leiskite sausainiams atvėsti ant kepimo skardos kelias minutes, tada perkelkite ant grotelių, kad visiškai atvėstų."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei norite stipresnio migdolų skonio, į tešlą galite pridėti keletą lašų migdolų ekstrakto."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Sausainiai bus minkšti išėmus iš orkaitės, bet sukietės atvėsę."
      },
