  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Pupelių paruošimas"
      },
      "text": {
        "lt": "Gerai nuplaukite pupeles, pašalinkite bet kokius nešvarumus ar akmenukus. Sudėkite pupeles į puodą, užpilkite pakankamai vandens ir virkite ant vidutinės ugnies, kol suminkštės ir vanduo beveik susigers (apie 40–60 minučių)."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Pagrindinio mišinio paruošimas"
      },
      "text": {
        "lt": "Į virtas pupeles įdėkite smulkintus svogūnus, pomidorus, čili ir aliejų. Gerai išmaišykite ir leiskite troškintis dar 10–15 minučių, kol mišinys sutirštės ir skoniai susijungs."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Papildomų ingredientų pridėjimas"
      },
      "text": {
        "lt": "Įmaišykite smulkintus lapus, krevečių miltelius, prieskonių kubelius ir druską. Toliau virkite dar 5–10 minučių, kartais pamaišydami, kol košė sutirštės ir gerai susimaišys."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite karštą, galima valgyti vieną arba su duona, keptais bananais ar kukurūzų pudingu."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei naudojate rudas pupeles, gali prireikti kiek ilgesnio virimo laiko nei juodosioms pupelėms."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Palmių aliejus suteikia tradicinį skonį, o žemės riešutų aliejus lengvesnį."
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Pridedant krevečių miltelius, sustiprėja sūrus skonis, ypač jei nenaudojama mėsa."
      },
      "priority": 3
    }
  ],
