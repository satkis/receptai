  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Ištirpinkite margariną ir cukrų, tada įmaišykite sausus ingredientus. Po to įmuškite kiaušinį ir gerai išplakite."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Papildymai"
      },
      "text": {
        "lt": "Į masę įmaišykite razinas ir riešutus, kol viskas gerai susijungs."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Kepimas orkaitėje"
      },
      "text": {
        "lt": "Kepti 180°C temperatūroje apie 50 minučių, naudojant riebalais išteptą formą su kepimo popieriumi apačioje."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Dekoravimas"
      },
      "text": {
        "lt": "Sumaišykite visus glajaus ingredientus dubenėlyje, kol gausite vienodą masę. Kai tortas atvės, užtepkite glajų ant torto viršaus."
      }
    }
  ],
  "notes": [
    {
      "text": {
