  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Alyvos paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų mažame puode iki 4–5 cm gylio vidutinės ugnies."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Sumaišykite pieno miltelius, Bisquick mišinį ir ištirpintą sviestą metaliniame dubenyje."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos maišymas"
      },
      "text": {
        "lt": "Palaipsniui, po 2 šaukštus, įpilkite šiltą pieną. Mediniu šaukštu maišykite, kol tešla taps tvirta ir drėgna, bet nelipni."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Formavimas"
      },
      "text": {
        "lt": "Suformuokite vienodus kamuoliukus. Nelaukite ilgai, nes jie greitai išdžiūsta."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įmeskite kelis kamuoliukus į įkaitintą aliejų. Maišykite, kad tolygiai ruduotų."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Perkėlimas"
      },
      "text": {
        "lt": "Kai taps tamsiai rudi, išimkite iš aliejaus ir padėkite ant popieriaus, kad sugertų aliejaus perteklių."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Sirupo paruošimas"
      },
      "text": {
        "lt": "Sulaužykite kardamono ankštis, išimkite sėklas ir sutrinkite jas. Sumaišykite su vandeniu ir cukrumi, kaitinkite 10 minučių."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Mirkymas"
      },
      "text": {
        "lt": "Įdėkite keptus kamuoliukus į sirupą ir palikite per naktį."
      }
    },
    {
      "step": 9,
      "name": {
        "lt": "Patiekti"
      },
      "text": {
        "lt": "Patiekite šiltus ar šaltus pagal skonį."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei tešla per lipni, įpilkite daugiau pieno miltelių."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Gulab jamun skaniausi, kai patiekiami su ledais."
      },
