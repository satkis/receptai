  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Tešlos maišymas"
      },
      "text": {
        "lt": "Sumaišykite aliejų, šokoladą ir cukrų. Gerai išmaišykite, kad visi ingredientai susijungtų."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kiaušinių pridėjimas"
      },
      "text": {
        "lt": "Vienu metu įmaišykite po vieną kiaušinį, kol masė bus gerai sumaišyta. Įpilkite vanilės."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Sausų ingredientų pridėjimas"
      },
      "text": {
        "lt": "Į šokolado mišinį įmaišykite miltus, kepimo miltelius ir druską. Viską gerai išmaišykite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos atšaldymas"
      },
      "text": {
        "lt": "Tešlą atšaldykite kelias valandas arba per naktį, kad ji sutvirtėtų."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimo pasiruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 175°C."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Pyragėlių formavimas"
      },
      "text": {
        "lt": "Šaukšteliu paimkite tešlos, apvoliokite cukraus pudroje ir suformuokite rutuliukus."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Rutuliukus dėkite ant riebalais pateptos kepimo skardos, palikdami tarp jų maždaug 5 cm tarpus. Kepkite 10-12 minučių."
      }
    }
  ],
