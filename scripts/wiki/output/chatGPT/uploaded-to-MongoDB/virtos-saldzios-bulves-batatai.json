  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": ".Supjaustykite saldžias bulves dydžio panašaus, kaip nuotraukoje. Į puodą sudėkite saldžiųjų bulvių gabaliukus. Užpildykite vandeniu, pridėkite žiupsnelį druskos."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Virinimas"
      },
      "text": {
        "lt": "Užvirinkite vandenį, tada sumažinkite ugnį ir virkite, kol bulvės suminkštės."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Nupilkite vandenį, nulupkite bulvių odelę. Patiekite su pasirinktu pipirų padažu."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite naudoti įvairius padažus pagal savo skonį."
      },
