  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošti tešlą"
      },
      "text": {
        "lt": "Sumaišykite miltus, cukrų ir druską dideliame dubenyje."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Mielių paruošimas"
      },
      "text": {
        "lt": "Mažame dubenyje sumaišykite vandenį ir cukrų, tada užberkite mieles. Palaukite 8 minutes, kol susidarys putos."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos maišymas"
      },
      "text": {
        "lt": "Į miltų mišinį įpilkite mielių mišinį, kiaušinius ir aliejų. Įpilkite likusį vandenį ir minkykite tešlą apie 10 minučių."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Pakilimas"
      },
      "text": {
        "lt": "Palikite tešlą kilti uždengtą 2–4 valandoms."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Duonos formavimas"
      },
      "text": {
        "lt": "Padalinkite tešlą į keturias dalis ir suformuokite ilgus virvės. Susukite jas į pynę."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Papildomi ingredientai"
      },
      "text": {
        "lt": "Aptepkite kiaušinio plakiniu ir apibarstykite sezamo sėklomis."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 200°C ir kepkite 25 minutes."
      }
    }
  ],
  "notes": [
    {
      "text": {
