  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Kremo paruošimas"
      },
      "text": {
        "lt": "Sumaišykite cukrų, kiaušinius ir semoliną puode. Įpilkite pieno ir vanilės ankštis. Įmaišykite citrinos sultis."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kremo virimas"
      },
      "text": {
        "lt": "Užvirinkite mišinį, visą laiką maišydami. Kai pradės virti, sumažinkite ugnį ir maišykite dar 10 minučių. Atvėsinkite mišinį."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Ištepkite tirpintu sviestu kiekvieną filo tešlos lapą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Pyrago formavimas"
      },
      "text": {
        "lt": "Sviestu patepkite kepimo formą ir įdėkite pusę filo tešlos. Užpilkite atvėsintą semolinos kremą ir uždenkite likusiais filo tešlos lakštais."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Pabarstykite pyragą šiek tiek vandens ir kepkite 180°C temperatūroje 1 valandą. Atvėsinkite."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Sirupo paruošimas"
      },
      "text": {
        "lt": "Sumaišykite vandenį ir cukrų puode. Užvirinkite ir virkite apie 10 minučių. Atvėsinkite sirupą ir įmaišykite citrinos sultis."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Pyragą aptepkite sirupu ir patiekite."
      }
    }
  ],
  "notes": [
    {
