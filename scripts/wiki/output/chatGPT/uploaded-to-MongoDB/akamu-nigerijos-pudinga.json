  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Pasiruošimas"
      },
      "text": {
        "lt": "Didelėje dubenyje sudėkite akamu pastą. Pasirinkite pakankamai didelį indą, nes akamu išsiplečia."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Lumpų sutrupinimas"
      },
      "text": {
        "lt": "Sutrupinkite akamu gabalėlius šaukštu ir sumaišykite su šaltu vandeniu iki vientisos masės."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Vandens užvirinimas"
      },
      "text": {
        "lt": "Užvirkite didelį puodą vandens."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Mišinio užpilimas"
      },
      "text": {
        "lt": "Kai vanduo užvirs, atsargiai supilkite jį į akamu mišinį, maišydami, kol masė pradės tirštėti."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Pabaiga"
      },
      "text": {
        "lt": "Įmaišykite garintą pieną ir cukrų pagal skonį. Jei reikia, pridėkite daugiau vandens."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei norite skystesnės konsistencijos, pridėkite daugiau karšto vandens."
