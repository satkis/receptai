  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Vyno pašildymas"
      },
      "text": {
        "lt": "Supilkite vyną į puodą ir pašildykite iki maždaug 70 °C."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Vaisių paruošimas"
      },
      "text": {
        "lt": "Nuplaukite vaisius karštu vandeniu, nulupkite apelsinus ir citrinas, ir pridėkite žieveles į vyną kartu su prieskoniais."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Cukraus uždegimas"
      },
      "text": {
        "lt": "Ant puodo uždėkite feuerzange ir ant jo padėkite Zuckerhut. Supilkite romą ant cukraus ir atsargiai uždekite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Romos pridėjimas"
      },
      "text": {
        "lt": "Kai liepsna užgęsta, lėtai pridėkite dar šiek tiek romo, kol cukrus visiškai ištirps."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Gėrimo užbaigimas"
      },
      "text": {
        "lt": "Pašalinkite vaisių žieveles, gvazdikėlius ir cinamono lazdelę prieš patiekdami."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Romas turi būti bent 50% stiprumo, kad liepsnotų."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Jei neturite Zuckerhut, galite naudoti cukraus kubelius."
      },
