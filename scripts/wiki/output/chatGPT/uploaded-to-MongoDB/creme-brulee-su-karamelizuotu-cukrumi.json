  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Orkaitės paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 150°C. Išdėliokite formeles dideliame kepimo inde su virtuviniu rankšluosčiu dugne."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Grietinėlės mišinys"
      },
      "text": {
        "lt": "Sumaišykite dalį grietinėlės su cukrumi ir vanilės sėklomis. Užvirinkite ir palikite įsigerti skonį 20 minučių."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Trynių paruošimas"
      },
      "text": {
        "lt": "Trynius plakite, kol jie taps vientisi. Įpilkite šiek tiek grietinėlės mišinio ir gerai išmaišykite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Mišinio pylimas į formeles"
      },
      "text": {
        "lt": "Perkoškite mišinį ir supilkite į formeles. Uždenkite aliuminio folija."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kepkite, kol kremas sutvirtės. Atvėsinkite ir laikykite šaldytuve."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Karamelizavimas"
      },
      "text": {
        "lt": "Pabarstykite cukrumi ir karamelizuokite su degikliu prieš patiekiant."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Svarbu, kad grietinėlė nebūtų per karšta, kai įdedate ją į trynius."
      },
