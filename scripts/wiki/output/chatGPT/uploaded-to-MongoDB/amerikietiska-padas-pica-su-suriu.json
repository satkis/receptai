  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Mielių aktyvavimas"
      },
      "text": {
        "lt": "Sumaišykite mieles su šiltu vandeniu ir palikite kelias minutes, kad suaktyvėtų."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Įmaišykite miltus, aliejų, cukrų ir druską į mielių mišinį. Įpilkite likusius miltus ir vandenį, maišykite iki vientisos tešlos. Jei reikia, pridėkite papildomai vandens ar miltų."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos kildinimas"
      },
      "text": {
        "lt": "Palikite tešlą kilti šiltoje vietoje, kol ji padvigubės. Tada perkelkite ant miltuoto paviršiaus ir suformuokite picos pado formą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Picos formavimas"
      },
      "text": {
        "lt": "Iškočiokite tešlą ir padėkite ant picos skardos, suformuokite storą kraštą."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Pagardų dėjimas"
      },
      "text": {
        "lt": "Užtepkite picos padažą ant tešlos, palikdami kraštus laisvus. Pabarstykite sūrį ir kitus norimus pagardus."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 220°C ir kepkite picą apie 25–30 minučių, kol sūris ir kraštai taps auksiniai."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Po kepimo palikite picą atšalti kelias minutes, tuomet supjaustykite gabalėliais ir patiekite su mėgstamais užkandžiais."
      }
    }
  ],
