  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruoškite kiaušinius"
      },
      "text": {
        "lt": "Užpilkite kiaušinius verdančiu vandeniu ir palikite 20 minučių, kad jie išliktų minkšti. Atsargiai nulupkite."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Paruoškite jautieną"
      },
      "text": {
        "lt": "Užvirkite jautieną ir nupilkite vandenį."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Paruoškite padažą"
      },
      "text": {
        "lt": "Sumaišykite sake, miriną, sojų padažą ir cukrų keptuvėje. Kaitinkite vidutinėje temperatūroje."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Troškinkite jautieną"
      },
      "text": {
        "lt": "Į padažą įdėkite jautieną ir troškinkite. Prieš išgaruojant visam padažui, išimkite jautieną."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Baigiamasis troškinimas"
      },
      "text": {
        "lt": "Pridėkite vandenį, dashi, tarkuotą imbierą ir svogūnus į padažą. Uždenkite ir užvirkite."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Sumaišykite ingredientus"
      },
      "text": {
        "lt": "Sumaišykite jautieną su padažu."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Patiekite"
      },
      "text": {
        "lt": "Įdėkite virtus ryžius į dubenį, ant viršaus uždėkite troškintą jautieną su padažu. Ant viršaus uždėkite minkštą kiaušinį ir marinuotą imbierą. Papuoškite svogūnlaiškiais ir shichimi pagardais."
      }
    }
  ],
