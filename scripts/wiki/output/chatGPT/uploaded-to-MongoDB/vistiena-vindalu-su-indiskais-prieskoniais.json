  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Svogūnų paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite aliejų keptuvėje ir apkepkite plonai pjaustytus svogūnus, kol jie taps auksiniai ir traškūs."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Svogūnų padažo gamyba"
      },
      "text": {
        "lt": "Kai svogūnai auksiniai, sutrinkite juos su šiek tiek aliejaus iki pastos konsistencijos. Tai bus pagrindinis padažas."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Česnako ir čili padažas"
      },
      "text": {
        "lt": "Sutrinkite česnaką, imbierą ir čili pipirus. Įpilkite ciberžolės, kalendros, garam masalos ir cinamono. Gerai išmaišykite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Vištienos paruošimas su prieskoniais"
      },
      "text": {
        "lt": "Įkaitinkite keptuvę su aliejumi, įdėkite vištieną ir kepkite su česnako bei čili padažu, kol vištiena bus beveik iškepusi."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Galutinis patiekalo paruošimas"
      },
      "text": {
        "lt": "Į vištieną įpilkite svogūnų padažą ir vandens. Sumažinkite ugnį ir troškinkite apie 30 minučių. Patiekite su ryžiais ir pabarstykite kalendra."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei neturite šviežių ingredientų, naudokite džiovintus, bet tai paveiks skonį."
