  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Mielės"
      },
      "text": {
        "lt": "Sumaišykite mieles ir cukrų pusėje puodelio vandens. Leiskite pastovėti 10 minučių, kol mielės sukils."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Miltus sumaišykite su druska dubenyje. Padarykite duobutę, supilkite aliejų ir mielių mišinį. Palaipsniui įpilkite likusį vandenį, kol susidarys minkšta tešla. Minkykite 5 minutes. Uždenkite ir leiskite kilti 1½ valandos."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Kiaušinio plakinio paruošimas"
      },
      "text": {
        "lt": "Sumaišykite kiaušinio trynį su 1 šaukštu vandens, kad paruoštumėte plakinį. Atidėkite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Formavimas ir kepimas"
      },
      "text": {
        "lt": "Padalinkite tešlą į 8 dalis ir suformuokite kamuoliukus. Kiekvieną iškočiokite į ovalą 20 cm ilgio ir 5 cm storio. Ant viršaus padarykite linijas, aptepkite plakiniu ir pabarstykite juodaisiais kuminai."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kepkite iš anksto įkaitintoje orkaitėje 175°C temperatūroje 25 minutes. Bendras kepimo laikas apie 2½ valandos."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei neturite juodojo kumino galite naudoti kmynus."
      },
      "priority": 1
    }
