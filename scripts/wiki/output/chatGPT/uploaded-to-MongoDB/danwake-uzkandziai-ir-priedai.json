  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Potašo paruošimas"
      },
      "text": {
        "lt": "Potašą ištirpinkite mažame dubenyje su trupučiu vandens ir atidėkite į šalį."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Miltų maišymas"
      },
      "text": {
        "lt": "Dideliame dubenyje sumaišykite kviečių miltus (arba pasirinktus miltus) su baobabo lapų miltais."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Skysto mišinio ruošimas"
      },
      "text": {
        "lt": "Lėtai į miltų mišinį pilkite ištirpintą potašo tirpalą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos formavimas"
      },
      "text": {
        "lt": "Pamažu pilkite vandenį, maišydami, kol susidarys tiršta, vientisa tešla."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Danwake virimas"
      },
      "text": {
        "lt": "Dideliame puode užvirkite didelį kiekį vandens. Naudodami šaukštą ar ranką, formuokite mažus tešlos gabaliukus ir dėkite juos į verdantį vandenį."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Virimo pabaiga"
      },
      "text": {
        "lt": "Leiskite virtinukams virti 10–15 minučių, kol jie pakils į paviršių ir taps tvirti."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Nukoškite virtinukus ir apšlakstykite augaliniu aliejumi. Patiekite su kapotais svogūnais ir kopūstais."
      }
    }
  ],
