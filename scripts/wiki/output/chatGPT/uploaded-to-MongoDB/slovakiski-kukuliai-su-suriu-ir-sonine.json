  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Sumaišykite tarkuotas bulves su miltais ir druska, kol gausite tešlą."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kukulių formavimas"
      },
      "text": {
        "lt": "Suformuokite mažus cilindrinius kukulius, mažesnius nei 3 cm."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Kukulių virimas"
      },
      "text": {
        "lt": "Sudėkite kukulius į verdantį pasūdytą vandenį. Kai kukuliai iškyla į paviršių, patikrinkite, ar jie visiškai išvirę."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Išimkite kukulius iš puodo ir perkelkite į pašildytą lėkštę. Užberkite bryndza sūrį ir leiskite jam ištirpti ant kukulių. Pabarstykite keptą šoninę ir patiekite karštus."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei neturite laiko, galite naudoti parduotuvėje nusipirktus bulvių gnocchi."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Jei nerandate bryndza, galite naudoti sutrupintą ožkos sūrį arba fetą."
