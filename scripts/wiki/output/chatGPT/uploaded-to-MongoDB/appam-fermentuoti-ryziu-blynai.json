  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Ryžių mirkymas"
      },
      "text": {
        "lt": "Mirkykite nevirtus ryžius vandenyje, kol jie suminkštės."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Malimas"
      },
      "text": {
        "lt": "Sumalkite mirkytus ryžius, kol jie bus maždaug ketvirtadaliais sumalti."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Kokoso įmaišymas"
      },
      "text": {
        "lt": "Įdėkite tarkuotą kokosą ir truputį vandens, tęskite malimą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Pridėkite cukrų, virtus ryžius ir mieles, ir toliau malkite, kol mišinys bus lygus ir skystesnis nei blynų tešla."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Fermentacija"
      },
      "text": {
        "lt": "Perkelkite į platų indą ir palikite nakčiai fermentuotis."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Blynų kepimas"
      },
      "text": {
        "lt": "Keptuvėje su šiek tiek aliejaus ar ghee sviestu kepkite blynus, kol jie taps auksiniai kraštuose."
      }
    }
  ],
