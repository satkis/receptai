  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Sviestą ir cukrų sumaišykite dubenyje, kol masė taps vienalytė."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kiaušinių ir vanilės įmaišymas"
      },
      "text": {
        "lt": "Įdėkite kiaušinį ir vanilę bei gerai išmaišykite."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Sausų ingredientų įmaišymas"
      },
      "text": {
        "lt": "Sumaišykite miltus, druską ir kepimo sodą. Šią sausą masę įmaišykite į šlapią masę."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Šokolado gabaliukų įmaišymas"
      },
      "text": {
        "lt": "Įmaišykite šokolado gabaliukus."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Šaukštu dėkite tešlos ant kepimo popieriumi padengtos skardos. Kepkite 180°C temperatūroje, kol sausainiai taps auksinės spalvos."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Atvėsinimas"
      },
      "text": {
        "lt": "Leiskite sausainiams šiek tiek atvėsti, tada perkelkite juos nuo skardos. Visiškai atvėsinkite."
      }
    }
  ],
