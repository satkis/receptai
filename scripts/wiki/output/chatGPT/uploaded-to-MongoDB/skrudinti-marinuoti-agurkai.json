  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruoškite pieno mišinį"
      },
      "text": {
        "lt": "Sumaišykite pieną, kiaušinį, miltus, Worcestershire padažą ir aštrų padažą. Gerai išmaišykite ir atidėkite į šalį."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Paruoškite miltų mišinį"
      },
      "text": {
        "lt": "Kitame dubenyje sumaišykite miltus, druską ir pipirus."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Apvoliokite agurkus"
      },
      "text": {
        "lt": "Pamerkite marinuotus agurkus į pieno mišinį, tada apvoliokite miltų mišinyje. Pakartokite procesą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Kepkite agurkus"
      },
      "text": {
        "lt": "Įkaitinkite aliejų iki 175 °C ir giliai kepkite agurkus, kol jie iškyla į paviršių ir tampa auksiniai. Nusausinkite ant popierinių rankšluosčių."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekite"
      },
      "text": {
        "lt": "Pabarstykite agurkus džiovintis krapais pagal skonį. Patiekite šiltus."
      }
    }
