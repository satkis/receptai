  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Atsargiai nuimkite riebalus nuo sultinio ir paragaukite, ar reikia druskos. Jei patieksite šaltą, truputį padidinkite druskos kiekį."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kiaušinių baltymų paruošimas"
      },
      "text": {
        "lt": "Dideliame dubenyje sumaišykite 1 puodelį sultinio su kiaušinių baltymais."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Sultinio kaitinimas"
      },
      "text": {
        "lt": "Įkaitinkite likusį sultinį iki virimo temperatūros ir po truputį supilkite į kiaušinių baltymų mišinį, nuolat maišydami."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Filtravimas"
      },
      "text": {
        "lt": "Kai sultinys pradeda virti, nustokite maišyti. Perkoškite per marle išklotą sietą į dubenį."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Pabaiga"
      },
      "text": {
        "lt": "Į atvėsintą sultinį įpilkite Madeira vyno ir patiekite."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei norite sodresnio skonio, naudokite jautienos sultinį."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Patiekite su šviežiai kepta duonele."
      },
      "priority": 2
    }
