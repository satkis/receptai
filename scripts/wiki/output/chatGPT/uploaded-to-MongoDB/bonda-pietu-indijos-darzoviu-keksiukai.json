  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Užvirkite bulves, nulupkite ir sutrinkite jas grubiai."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Daržovių kepimas"
      },
      "text": {
        "lt": "Įkaitinkite 2 šaukštus aliejaus keptuvėje. Kai aliejus įkaitęs, sudėkite žalią čili ir kepkite kelias sekundes."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Svogūnų pridėjimas"
      },
      "text": {
        "lt": "Įpilkite svogūnus ir kepkite, kol jie taps rusvos spalvos."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Prieskonių įdėjimas"
      },
      "text": {
        "lt": "Įdėkite imbierą ir česnaką, kepkite dar minutę."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Masės formavimas"
      },
      "text": {
        "lt": "Įdėkite koriandro lapus ir sutrintas bulves, gerai sumaišykite. Leiskite atvėsti, tada suformuokite mažus kamuoliukus."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Sumaišykite miltus ir ryžių miltus su kepimo soda. Įpilkite druskos pagal skonį."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Įpilkite pakankamai vandens ar alaus, kad tešla būtų šiek tiek tirštesnė nei kečupas."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kai tešla ir įdaras paruošti, įkaitinkite aliejų gilioje keptuvėje. Aliejaus turėtų būti bent 4 cm gylio."
      }
    },
    {
      "step": 9,
      "name": {
        "lt": "Kepimo procesas"
      },
      "text": {
        "lt": "Panardinkite kamuoliukus į tešlą, kad jie būtų visiškai padengti, ir įdėkite į karštą aliejų. Neperkraukite keptuvės."
      }
    },
    {
      "step": 10,
      "name": {
        "lt": "Pateikimas"
      },
      "text": {
        "lt": "Keptuvėje keptus bondas kepkite, kol jie taps auksinės spalvos, tada išimkite iš aliejaus. Patiekite su kečupu arba čatniu."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Norint gauti ypatingą skonį, galite naudoti alų vietoj vandens tešlai."
      },
      "priority": 1
    }
  ],
