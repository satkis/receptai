  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Riešutų ir datulių paruošimas"
      },
      "text": {
        "lt": "Sumalkite riešutus ir datules. Įmaišykite baltąjį cukrų."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kiaušinių baltymų plakimas"
      },
      "text": {
        "lt": "Kitame dubenėlyje suplakite kiaušinių baltymus iki standumo."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Mišinio paruošimas"
      },
      "text": {
        "lt": "Pamažu įplakite cukraus mišinį, nuolat plakant. Įmaišykite grietinėlę."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos maišymas"
      },
      "text": {
        "lt": "Persijokite miltus, tada persijokite su kepimo milteliais. Įmaišykite į kiaušinių mišinį."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Sutepkite ir pabarstykite miltais kepimo skardą. Tešlą suformuokite 1.25 cm storio ant skardos ir kepkite 10 minučių 175°C temperatūroje."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Glajaus paruošimas"
      },
      "text": {
        "lt": "Sumaišykite cukraus pudrą su citrinų sultimis ir vanile."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Glajaus užtepimas"
      },
      "text": {
        "lt": "Kol batoneliai dar karšti, užtepkite glajų ant jų."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Batonelius galite laikyti sandariame inde iki savaitės."
