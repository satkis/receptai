  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Sudedamųjų dalių maišymas"
      },
      "text": {
        "lt": "Sumaišykite pieno miltelius, cukrų, kukurūzų krakmolą ir vanilės miltelius dubenyje. Gerai išmaišykite."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Įpilkite pakankamai vandens, kad susidarytų tešla. Gerai išmaišykite, kol tešla taps vientisa."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Kūgučių formavimas"
      },
      "text": {
        "lt": "Sutepkite rankas augaliniu aliejumi ir suformuokite mažus tešlos rutuliukus."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite augalinį aliejų keptuvėje ir kepkite tešlos rutuliukus, kol jie taps auksinės spalvos."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Išimkite kūgucius iš aliejaus, nusausinkite ant popierinių rankšluosčių ir patiekite šviežius."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Į tešlą galite pridėti čili pipirų, anakardžių riešutų arba kokoso, kad sukurtumėte skirtingus skonius."
