  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Mielių paruošimas"
      },
      "text": {
        "lt": "Sumaišykite cukrų su šiltu vandeniu ir pabarstykite mieles. Leiskite pastovėti apie 10 minučių."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos ruošimas"
      },
      "text": {
        "lt": "Dideliame dubenyje išplakite kiaušinius. Įpilkite pašildytą pieną, šiltą vandenį, mielių mišinį ir margariną. Išmaišykite."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Miltų maišymas"
      },
      "text": {
        "lt": "Į masę įdėkite miltus, cukrų ir druską. Gerai išmaišykite. Įdėkite supjaustytus obuolius."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos kilimas"
      },
      "text": {
        "lt": "Leiskite tešlai pakilti šiltoje vietoje, kol padvigubės. Tam puikiai tinka didelis puodas."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Cinamoninio cukraus paruošimas"
      },
      "text": {
        "lt": "Atskirame dubenyje sumaišykite cukrų su cinamonu pagal skonį ir atidėkite."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Užpildykite keptuvę aliejumi ir įkaitinkite iki 190°C. Į karštą aliejų dėkite tešlos su šaukštu."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Spurgų kepimas"
      },
      "text": {
        "lt": "Kepkite spurgas, kol taps auksinės spalvos, vartykite, kad tolygiai iškeptų."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Spurgų išėmimas"
      },
      "text": {
        "lt": "Išimkite spurgas ir padėkite ant popierinių rankšluosčių."
      }
    },
    {
      "step": 9,
      "name": {
        "lt": "Spurgų apvoliojimas"
      },
      "text": {
        "lt": "Apvoliokite spurgas cinamoniniame cukruje ir patiekite šiltas."
      }
    }
  ],
