  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Sausų ingredientų sumaišymas"
      },
      "text": {
        "lt": "Giliame dubenyje sumaišykite miltus, druską ir cukrų."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Sviesto įmaišymas"
      },
      "text": {
        "lt": "Supjaustykite sviestą mažais gabalėliais ir įmaišykite į miltų mišinį."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos suformavimas"
      },
      "text": {
        "lt": "Įpilkite šaltą vandenį ir maišykite, kol tešla taps vienalytė ir ją galima bus suformuoti į rutulį."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos atšaldymas"
      },
      "text": {
        "lt": "Suformuotą tešlą įvyniokite į plėvelę ir padėkite į šaldytuvą 10–20 minučių."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Tešlos iškočiojimas"
      },
      "text": {
        "lt": "Iškočiokite tešlą ant lengvai miltuoto paviršiaus, kad ji būtų tinkamo dydžio pyrago formai."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Pyrago formavimas"
      },
      "text": {
        "lt": "Tešlą dėkite į pyrago formą, supildami norimą įdarą, uždenkite antru tešlos sluoksniu."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kepkite pyragą 30 minučių 220 °C temperatūroje, tada sumažinkite temperatūrą iki 180 °C ir kepkite dar 30 minučių."
      }
    }
  ],
