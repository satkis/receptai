  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Ingredientų paruošimas"
      },
      "text": {
        "lt": "Supjaustykite pomidorus, agurkus, svogūnus ir paprikas. Sudėkite juos į didelį dubenį kartu su alyvuogėmis."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Pagardinimas"
      },
      "text": {
        "lt": "Įpilkite alyvuogių aliejaus, acto ar citrinos sulčių, ir pagardinkite druska. Gerai išmaišykite, kad visi ingredientai pasidengtų padažu."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Ant salotų viršaus uždėkite fetos sūrį ir apšlakstykite alyvuogių aliejumi. Jei norite, papuoškite raudonėliu."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei norite intensyvesnio skonio, naudokite daugiau alyvuogių aliejaus ir prieskonių."
      },
