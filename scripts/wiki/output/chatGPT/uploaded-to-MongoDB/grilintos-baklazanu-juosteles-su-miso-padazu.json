  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Baklažanų paruošimas"
      },
      "text": {
        "lt": "Nuplaukite baklažaną ir supjaustykite jį 2,5 cm storio juostelėmis išilgai. Aptepkite alyvuogių aliejumi."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Grilinimas"
      },
      "text": {
        "lt": "Grilinkite baklažanus 7-10 cm nuo tiesioginės ugnies, kol jie taps minkšti ir apskrudę (apie 5 minutes)."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Padažo paruošimas"
      },
      "text": {
        "lt": "Mažame puode sumaišykite miso pastą, cukrų ir sultinio kubelius. Kaitinkite ant mažos ugnies, kol mišinys taps sklandus ir karštas. Neužvirinkite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Padažo užpylimas"
      },
      "text": {
        "lt": "Užpilkite padažą ant grilintų baklažanų."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Sezamų sėklų paskrudinimas"
      },
      "text": {
        "lt": "Mažoje keptuvėje paskrudinkite sezamų sėklas, kol jos pradeda šokti. Pabarstykite jomis baklažanus."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite naudoti ir kitokį aliejų, pavyzdžiui, sezamų ar saulėgrąžų."
