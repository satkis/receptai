  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Kokoso paruošimas"
      },
      "text": {
        "lt": "Sumaišykite kokosų drožles su žiupsneliu druskos ir padėkite į šoną. Tai bus naudinga vėliau."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Pandano paruošimas"
      },
      "text": {
        "lt": "Naudokite blenderį arba grūstuvę, kad sutrintumėte pandano lapus su vandeniu. Nukoškite mišinį ir pašalinkite kietas dalis."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos ruošimas"
      },
      "text": {
        "lt": "Sumaišykite ryžių miltus su pandano skysčiu, kad susidarytų tešla. Gerai išminkykite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Koldūnų formavimas"
      },
      "text": {
        "lt": "Atsikirpkite tešlos gabalėlius ir suformuokite rutuliukus. Viduje padarykite nedidelę duobutę ir įdėkite palmių cukraus. Užspauskite tešlą aplink cukrų ir vėl suformuokite rutuliuką."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Virimui pasiruošimas"
      },
      "text": {
        "lt": "Užvirkite 2 litrus vandens giliame puode."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Koldūnų virimas"
      },
      "text": {
        "lt": "Įdėkite koldūnus į puodą, virkite kol jie iškils į paviršių. Nepervirkite, kad jie nesprogtų."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Pateikimas"
      },
      "text": {
        "lt": "Išimkite koldūnus ir nusausinkite. Kol jie dar karšti, apvoliokite kokosų drožlėse."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei neturite pandano lapų, galite naudoti vanilės esenciją."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Pasirūpinkite, kad palmių cukraus gabalėliai būtų pakankamai maži, kad tilptų į tešlą."
      },
      "priority": 2
    }
