  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Susmulkink svogūnus, svogūnų laiškus, česnaką, aštrias paprikas, sultinio kubelius ir druską."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Žemės riešutų sviesto paruošimas"
      },
      "text": {
        "lt": "Praskiesk žemės riešutų sviestą vandeniu, kad jis taptų skystas. Jei reikia, perkošk."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Vištienos paruošimas"
      },
      "text": {
        "lt": "Paskanink vištieną actu ir garstyčių milteliais."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Vištienos kepimas"
      },
      "text": {
        "lt": "Apkepk vištieną, kol paruduos iš abiejų pusių. Išimk ir atidėk."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Daržovių kepimas"
      },
      "text": {
        "lt": "Į keptuvę sudėk susmulkintą mišinį ir daržoves, kepk."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Troškinio užbaigimas"
      },
      "text": {
        "lt": "Pridėk žemės riešutų sviestą, pomidorų pastą, vištieną ir vandenį. Troškink, kol padažas sutirštės."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Patiekimas"
      },
