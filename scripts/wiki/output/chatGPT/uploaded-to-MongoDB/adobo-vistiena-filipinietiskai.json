  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Supjaustymas"
      },
      "text": {
        "lt": "Supjaustykite vištieną vidutinio dydžio gabaliukais."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Sumaišykite vištienos gabaliukus su actu, sojų padažu, česnakais, lauro lapais ir juodaisiais pipirais. Įdėkite į šaldytuvą marinuotis 30 minučių."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Išimkite vištieną iš marinato ir apkepkite keptuvėje ant vidutinės ugnies, kol vištiena bus auksinės spalvos."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Troškinimas"
      },
      "text": {
        "lt": "Į keptuvę įpilkite likusį marinatą, sumažinkite ugnį ir troškinkite vištieną 20-25 minutes, kol ji taps minkšta ir skysčiai sumažės."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Norėdami išskirtinio skonio, naudokite kokosų actą vietoj įprasto."
      },
