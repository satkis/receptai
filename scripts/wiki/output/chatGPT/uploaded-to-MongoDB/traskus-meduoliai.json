  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Mišinio paruošimas"
      },
      "text": {
        "lt": "Sumaišykite puode cukrų, melasą, prieskonius ir druską. Užvirinkite ir nuimkite nuo ugnies."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Sviesto ir sodos įmaišymas"
      },
      "text": {
        "lt": "Į karštą mišinį įmaišykite sodą ir sviestą. Maišykite, kol sviestas ištirps."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos užbaigimas"
      },
      "text": {
        "lt": "Greitai įmuškite kiaušinį ir tada įmaišykite miltus. Gerai išmaišykite. Atvėsinkite kelias valandas, kol tešla taps kieta."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 170 °C. Iškočiokite tešlą ant lengvai miltuoto paviršiaus ir išpjaukite formeles. Kepkite apie 15 minučių ir atvėsinkite ant grotelių."
      }
    }
  ],
