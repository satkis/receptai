  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Persijokite miltus į dubenį ir įtrinkite margariną, kol mišinys taps panašūs į miltus."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Kiaušinių ir cukraus maišymas"
      },
      "text": {
        "lt": "Atskirkite indą, sumaišykite kiaušinius su cukrumi iki vientisos masės."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Apelsinų žievelės pridėjimas"
      },
      "text": {
        "lt": "Į kiaušinių ir cukraus mišinį įmaišykite nutarkuotą apelsino žievelę."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Tešlos užbaigimas"
      },
      "text": {
        "lt": "Įmaišykite kiaušinių mišinį į miltų mišinį. Prireikus įpilkite šiek tiek vandens, kad tešla suformuotųsi."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Įdaro paruošimas"
      },
      "text": {
        "lt": "Mažame inde pašildykite vandenį ir ištirpinkite jame cukrų, tada įmaišykite maltus migdolus."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Figolli formavimas"
      },
      "text": {
        "lt": "Iškočiokite tešlą ir išpjaukite norimas figūras. Užtepkite įdarą ir uždenkite kita tešlos dalimi."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 190 °C ir kepkite figolli apie 30 minučių, kol taps aukso spalvos."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Naudokite metalines formeles, kad išgautumėte norimas figūras."
      },
