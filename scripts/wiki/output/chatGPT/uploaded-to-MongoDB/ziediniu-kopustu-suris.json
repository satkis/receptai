  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Supjaustykite žiedinius kopūstus į mažas dalis ir pamerkite į sūdytą vandenį valandai."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Virimas"
      },
      "text": {
        "lt": "Nusausinkite žiedinius kopūstus ir dėkite į verdantį pasūdytą vandenį. Virkite, kol suminkštės, bet dar išliks šiek tiek traškūs."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Orkaitės paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 200°C."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Sumaišymas"
      },
      "text": {
        "lt": "Išvirtus žiedinius kopūstus sudėkite į indą. Sumaišykite baltąjį padažą su 2/3 parmezano ir užpilkite ant kopūstų. Pabarstykite džiūvėsiais ir likusiu parmezanu."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Dėkite į orkaitę ir kepkite apie 20 minučių arba kol gražiai apskrus."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite iš karto, kol dar karšta."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite pridėti daugiau sūrio, kad patiekalas būtų dar sūresnis ir sotesnis."
      },
      "priority": 1
    }
  ],
