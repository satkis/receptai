  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Mirkyti pupeles"
      },
      "text": {
        "lt": "Išplaukite ir užmerkite juoduosius lęšius ir raudonas pupeles per naktį šiltame vandenyje."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Virimas"
      },
      "text": {
        "lt": "Virti mirkytas pupeles su vandeniu, druska, čili milteliais, ciberžole ir tarkuotu imbieru, kol jos taps minkštos."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Keptuvėje"
      },
      "text": {
        "lt": "Įkaitinkite ghee ar aliejų ir pridėkite hingą, lauro lapus, cinamono lazdelę, gvazdikėlius ir smulkintą svogūną. Kepkite, kol svogūnas taps auksinės spalvos. Tada pridėkite imbiero ir česnako pastą ir maišykite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Pomidorų pridėjimas"
      },
      "text": {
        "lt": "Pridėkite smulkintus pomidorus ir kalendros miltelius. Viską maišykite, kol pomidorai sukrenta."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Sujungimas"
      },
      "text": {
        "lt": "Pridėkite virtas pupeles ir, jei reikia, vandens. Gerai išmaišykite ir palikite virti 15-20 minučių."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Gaminimo užbaigimas"
      },
      "text": {
        "lt": "Pridėkite grietinę ir sviestą, ir dar kartą leiskite užvirti 5 minutes. Prieš patiekiant, papuoškite kalendros lapais."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite pakeisti grietinę rūgštesne grietine arba mažai riebalų turinčiu jogurtu."
