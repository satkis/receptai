  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Avokadų paruošimas"
      },
      "text": {
        "lt": "Perpjaukite avokadus per pusę ir išimkite kauliukus. Supjaustykite avokadų minkštimą kvadratėliais, neperpjaudami žievės."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Avokadų sutrynimas"
      },
      "text": {
        "lt": "Išimkite minkštimą dideliu šaukštu ir sudėkite į dubenį. Įpilkite laimo sulčių ir gerai išmaišykite, kad avokadai būtų tolygiai padengti."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Prieskonių pridėjimas"
      },
      "text": {
        "lt": "Pridėkite pico de gallo, česnaką, aliejų, jalapeño, druską ir maltus juoduosius pipirus. Gerai išmaišykite, sutrinkite ir sumaišykite avokado gabaliukus, kol ingredientai gerai susimaišys."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Guakamolės užbaigimas"
      },
      "text": {
        "lt": "Išimkite likusius avokadų gabaliukus ir švelniai įmaišykite į masę. Guakamolė yra tinkamos konsistencijos, kai daugiau didelių gabaliukų nei sutrintų dalių."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Pateikimas"
      },
      "text": {
        "lt": "Papuoškite guakamolę kalendros šakele ir patiekite su tortilijų traškučiais."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Guakamolė geriausiai tinka vartoti iš karto, tačiau laimo sultys ir aliejus padeda ją ilgiau išlaikyti šviežią."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Pridėkite graikiško jogurto arba trintų juodųjų pupelių, kad patiekalas būtų sodresnis ir turtingesnis baltymų."
      },
      "priority": 2
    },
    {
      "text": {
        "lt": "Norėdami išvengti oksidacijos, uždenkite guakamolės paviršių plastikine plėvele."
      },
      "priority": 3
    }
  ],
