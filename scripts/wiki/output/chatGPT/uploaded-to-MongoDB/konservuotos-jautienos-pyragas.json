  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Orkaitės paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 200°C temperatūros."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Persijokite miltus į dubenį. Įtrinkite taukus ir margariną."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos formavimas"
      },
      "text": {
        "lt": "Įpilkite tiek vandens, kad susidarytų tešla. Pusę tešlos iškočiokite ir ja išklokite didelį pyrago indą."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Bulvių paruošimas"
      },
      "text": {
        "lt": "Užvirkite bulves, nusausinkite, įdėkite sviestą ir sutrinkite."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Įdaras"
      },
      "text": {
        "lt": "Į bulves įmaišykite svogūną ir konservuotą jautieną."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Pyrago formavimas"
      },
      "text": {
        "lt": "Įdėkite bulvių mišinį į paruoštą indą. Iškočiokite likusią tešlos dalį ir uždenkite pyragą, kraštus gerai užspauskite."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Pyrago kepimas"
      },
      "text": {
        "lt": "Viršuje padarykite kelias įpjovas, aptepkite pienu ir kepkite, kol pyragas taps auksinės spalvos, apie 35-40 minučių."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Šis pyragas puikiai dera su šviežiomis daržovėmis ar bulvių koše."
      },
