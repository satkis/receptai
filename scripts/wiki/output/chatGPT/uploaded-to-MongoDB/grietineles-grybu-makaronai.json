  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Makaronų virimas"
      },
      "text": {
        "lt": "Išvirkite makaronus pagal pakuotės instrukcijas, kol bus al dente. Tuo tarpu pradėsi ruošti padažą."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Padažo paruošimas"
      },
      "text": {
        "lt": "Didelėje keptuvėje ištirpinkite sviestą ir apkepkite česnakus. Įdėkite grybus ir kepkite, kol suminkštės."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Padažo užbaigimas"
      },
      "text": {
        "lt": "Įpilkite vyno ir leiskite jam šiek tiek išgaruoti. Tada įpilkite grietinėlės ir vištienos sultinio. Pagardinkite druska ir pipirais."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Sumaišykite makaronus su padažu, suberkite parmezaną ir gerai išmaišykite. Papuoškite petražolėmis ir patiekite."
      }
    }
  ],
