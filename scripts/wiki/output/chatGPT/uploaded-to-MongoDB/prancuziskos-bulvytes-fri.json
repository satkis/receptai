  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Bulvių paruošimas"
      },
      "text": {
        "lt": "Nulupkite bulves ir supjaustykite jas plonais šiaudeliais. Nuplaukite jas šaltame vandenyje, kad pašalintumėte perteklinį krakmolą."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Bulvių virimas"
      },
      "text": {
        "lt": "Dėkite bulvytes į puodą su vandeniu ir virkite apie 15 minučių, kol jos bus beveik paruoštos."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Bulvių džiovinimas"
      },
      "text": {
        "lt": "Nusausinkite bulvytes ir gerai jas išdžiovinkite popieriniu rankšluosčiu."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Giliau keptuvėje įkaitinkite aliejų iki 175°C ir kepkite bulvytes 4-5 minutes, kol jos taps auksinės spalvos ir traškios."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Išimkite bulvytes iš aliejaus ir padėkite ant popierinio rankšluosčio, kad sugertų perteklinį aliejų. Pabarstykite druska ir patiekite."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galite eksperimentuoti su prieskoniais, pavyzdžiui, česnaku ar paprikos milteliais."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Patiekite su jūsų mėgstamu padažu, pavyzdžiui, majonezu ar kečupu."
      },
