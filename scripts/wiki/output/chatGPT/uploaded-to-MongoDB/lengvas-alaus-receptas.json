  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Guaranos paruošimas"
      },
      "text": {
        "lt": "Sutrinkite guaranos uogas ir užpilkite jas 1 litru karšto, bet ne verdančio vandens (maksimaliai 78 °C)."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Guaranos įmaišymas"
      },
      "text": {
        "lt": "Perkoškite mišinį ir įpilkite jį į verdantį misą paskutines 15 minučių."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Salyklo maišymas"
      },
      "text": {
        "lt": "Sutrinkite salyklą ir sumaišykite su 135 litrais vandens, laikykite 66 °C temperatūroje 60 minučių."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Temperatūros kėlimas"
      },
      "text": {
        "lt": "Pakelkite temperatūrą iki 72 °C ir laikykite 5 minutes, tada pakelkite iki 78 °C ir laikykite 10 minučių."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Sparginimas"
      },
      "text": {
        "lt": "Naudokite 15.5 litrų 78 °C vandens sparginimui ir surinkite 22.7 litrų misos."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Misos virimas"
      },
      "text": {
        "lt": "Virkite misą 60 minučių, įdėkite Guaranos mišinį paskutines 15 minučių ir Willamette apynius paskutines 7 minutes."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Vėsinimas ir fermentacija"
      },
      "text": {
        "lt": "Atvėsinkite misą iki 19.5 °C ir supilkite į švarų fermentatoriaus indą. Įdėkite mieles ir fermentuokite 7–10 dienų."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Karbonizavimas ir buteliavimas"
      },
      "text": {
        "lt": "Ištirpinkite cukrų karštame vandenyje ir įpilkite į fermentuotą alų. Supilkite alų į butelius, leiskite karbonizuotis 7–10 dienų 19.5 °C temperatūroje."
      }
    },
    {
      "step": 9,
      "name": {
        "lt": "Laikymas ir pateikimas"
      },
      "text": {
        "lt": "Laikykite butelius 4–8 °C temperatūroje 14–30 dienų, patiekite šaltą."
      }
    }
  ],
