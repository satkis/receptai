  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Kepimo paruošimas"
      },
      "text": {
        "lt": "Įkaitinkite orkaitę iki 175°C. Duoną suplėšykite į mažus gabaliukus arba supjaustykite kubeliais ir sudėkite į kepimo formą. Įmaišykite razinas."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Sviesto užpylimas"
      },
      "text": {
        "lt": "Ant duonos gabaliukų užpilkite tirpintą sviestą, kad jie būtų gerai padengti."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Mišinio paruošimas"
      },
      "text": {
        "lt": "Dubenyje lengvai išplakite kiaušinius, pridėkite cukrų, pieną, cinamoną ir vanilės ekstraktą. Gerai išmaišykite."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Mišinio užpylimas"
      },
      "text": {
        "lt": "Supilkite mišinį ant duonos ir švelniai paspauskite, kad duona gerai sugertų skystį."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kepkite įkaitintoje orkaitėje apie 45 minutes, kol pudingas taps auksinės spalvos."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Leiskite atvėsti ir patiekite šaltą arba šiltą. Galima patiekti su šaukštu arba supjaustyti kvadratėliais."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Jei neturite razinų, galite naudoti kitus džiovintus vaisius."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Pudingą galite patiekti su ledais arba plakta grietinėle."
      },
