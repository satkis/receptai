  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Tešlos paruošimas"
      },
      "text": {
        "lt": "Sumaišykite miltus ir kiaušinius, tada įpilkite vandens ir druskos. Minkykite tešlą, kol ji taps lygi, ir palikite pailsėti 1 valandai."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Įdaro paruošimas"
      },
      "text": {
        "lt": "Susmulkinkite svogūną, česnaką ir sumaišykite su malta jautiena, kiaušiniais, muskatiniu riešutu, druska ir pipirais."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Tešlos kočiojimas"
      },
      "text": {
        "lt": "Iškočiokite tešlą į stačiakampį. Tolygiai paskleiskite mėsos įdarą ir suvyniokite į cilindrą. Užsandarinkite kraštą vandeniu."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Supjaustykite ritinį riekelėmis ir pasikepkite aliejuje, kol pagels."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Virimas sultinyje"
      },
      "text": {
        "lt": "Įpilkite sultinio (ir vyno, jei naudojate) ir virkite apie 20 minučių."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Pateikimas"
      },
      "text": {
        "lt": "Patiekite su žaliosiomis salotomis. Skanaus!"
      }
    }
  ],
