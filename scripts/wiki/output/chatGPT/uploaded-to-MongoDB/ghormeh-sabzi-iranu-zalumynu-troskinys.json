  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Pupelių virimas"
      },
      "text": {
        "lt": "Užvirkite raudonąsias pupeles 1 valandą. Tuo metu galite pasiruošti kitus ingredientus."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Mėsos paruošimas"
      },
      "text": {
        "lt": "Pašalinkite nereikalingus riebalus iš mėsos ir supjaustykite ją mažesniais gabalėliais."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Svogūnų kepimas"
      },
      "text": {
        "lt": "Įkaitinkite alyvuogių aliejų dideliame puode ir apkepkite svogūnus aukštoje temperatūroje 1–2 minutes."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Mėsos kepimas"
      },
      "text": {
        "lt": "Įdėkite mėsą į puodą, kepkite dar 1–2 minutes, tada sumažinkite ugnį iki minimumo."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Žolelių pridėjimas"
      },
      "text": {
        "lt": "Į puodą įmaišykite petražoles, špinatus, kalendras ir ghormeh sabzi žoleles."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Vandens pridėjimas"
      },
      "text": {
        "lt": "Įpilkite verdantį vandenį (apie 0.5 L) ir gerai išmaišykite."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Pupelių pridėjimas"
      },
      "text": {
        "lt": "Įdėkite virtas pupeles į troškinį ir virkite ant mažos ugnies 30 minučių - 3 valandas."
      }
    },
    {
      "step": 8,
      "name": {
        "lt": "Džiovintų laimų paruošimas"
      },
      "text": {
        "lt": "Mirkinkite džiovintus laimus karštame vandenyje 15 minučių, tada pradurkite juos šakute."
      }
    },
    {
      "step": 9,
      "name": {
        "lt": "Prieskonių pridėjimas"
      },
      "text": {
        "lt": "Įdėkite džiovintus laimus, ciberžolę, druską ir pipirus į troškinį, virkite dar 30–60 minučių."
      }
    },
    {
      "step": 10,
      "name": {
        "lt": "Ryžių paruošimas"
      },
      "text": {
        "lt": "Išvirkite ryžius, susmulkinkite šafraną su cukrumi, įpilkite 1 šaukštą verdančio vandens ir sumaišykite su dalimi ryžių."
      }
    },
    {
      "step": 11,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite troškinį karštą, atskirame inde. Ryžius patiekite atskirai, ant viršaus uždėkite šafrano ryžius."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Mažesni džiovinti laimai turi geresnį skonį."
      },
