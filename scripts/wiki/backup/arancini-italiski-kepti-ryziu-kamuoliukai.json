  "instructions": [
    {
      "step": 1,
      "name": {
        "lt": "Ryžių virimas"
      },
      "text": {
        "lt": "Virti ryžius pasūdytame vandenyje, kol suminkštės. Nusausinkite ir atvėsinkite."
      }
    },
    {
      "step": 2,
      "name": {
        "lt": "Sūrių ir pipirų maišymas"
      },
      "text": {
        "lt": "Sumaišykite ryžius su mozzarella ir parmezano sūriais bei pipirais, palikite atvėsti."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Įdaro paruošimas"
      },
      "text": {
        "lt": "Kumpį ir sviestą sumaišykite. Tai bus įdaras."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Kamuoliukų formavimas"
      },
      "text": {
        "lt": "Suformuokite ryžių paplotėlį. įdėję įdarą ant paplotėlio - suformuokite kamuoliukus."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Kamuoliukų padengimas"
      },
      "text": {
        "lt": "Apvoliokite kamuoliukus miltuose, kiaušinių plakinyje, tada džiūvėsėliuose."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Kepimas"
      },
      "text": {
        "lt": "Kepkite kamuoliukus karštame aliejuje, kol taps auksinės spalvos. Nusausinkite ant popierinių rankšluosčių."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Patiekimas"
      },
      "text": {
        "lt": "Patiekite su šiltu marinaro padažu."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Kepant aliejaus lygis turėtų būti bent iki pusės kamuoliukų."
      },
