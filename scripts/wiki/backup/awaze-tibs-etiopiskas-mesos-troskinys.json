  "instructions": [
    {
      "step": 1,
      "name": { "lt": "Aliejaus įkaitinimas" },
      "text": { "lt": "Didelėje keptuvėje arba wok'e įkaitinkite aliejų ant vidutinės-aukštos ugnies." }
    },
    {
      "step": 2,
      "name": { "lt": "Mėsos kepimas" },
      "text": { "lt": "Sudėkite plonai pjaustytą mėsą ir kepkite 3–4 minutes, kol apskrus. Išimkite ir atidėkite." }
    },
    {
      "step": 3,
      "name": { "lt": "Svogūnų ir česnakų pakepinimas" },
      "text": { "lt": "Toje pačioje keptuvėje sudėkite svogūną ir česnaką. Kepkite 2–3 minutes, kol suminkštės ir taps aromatingi." }
    },
    {
      "step": 4,
      "name": { "lt": "Prieskonių įmaišymas" },
      "text": { "lt": "Įdėkite berberės prieskonius, pomidorų pastą, papriką ir kajeno pipirus. Išmaišykite." }
    },
    {
      "step": 5,
      "name": { "lt": "Mėsos grąžinimas į keptuvę" },
      "text": { "lt": "Grąžinkite mėsą ir gerai sumaišykite su prieskonių mišiniu." }
    },
    {
      "step": 6,
      "name": { "lt": "Padažo suvienodinimas" },
      "text": { "lt": "Įdėkite awaze padažą, sojų padažą, actą ir druską. Išmaišykite, kad mėsa pasidengtų padažu." }
    },
    {
      "step": 7,
      "name": { "lt": "Patiekimas" },
      "text": { "lt": "Nuimkite nuo ugnies, pabarstykite šviežiais kalendrais ir patiekite su injera ar ryžiais." }
    }
  ],
  "notes": [
    {
      "text": { "lt": "Aštrumą reguliuokite pagal skonį, pridėdami daugiau arba mažiau kajeno pipirų." },
      "priority": 1
    },
    {
      "text": { "lt": "Awaze padažą galima nusipirkti Afrikos/etiopų parduotuvėse arba pasigaminti namuose." },
      "priority": 2
    },
    {
      "text": { "lt": "Puikiai dera su injera, ryžiais arba duona." },
