  "instructions": [
      {
      "step": 1,
      "name": {
        "lt": "Duonos paruošimas"
      },
      "text": {
        "lt": "Supjaustykite duoną smulkenisias kubeliais."
      }
    },
  
    {
      "step": 2,
      "name": {
        "lt": "Paruošimas"
      },
      "text": {
        "lt": "Sumaišykite pieną, kiaušinius ir druską. Supjaustykite duoną smulkenisias kubeliais. Supilkite mišinį ant duonos kubelių kartu su petražolėmis ir gerai išmaišykite. Palikite trumpam pastovėti."
      }
    },
    {
      "step": 3,
      "name": {
        "lt": "Svogūnų kepimas"
      },
      "text": {
        "lt": "Smulkiai supjaustykite svogūnus ir kepkite svieste, kol taps skaidrūs. Leiskite atvėsti ir įmaišykite į duonos mišinį."
      }
    },
    {
      "step": 4,
      "name": {
        "lt": "Masės paruošimas"
      },
      "text": {
        "lt": "Palikite masę valandai, kartkartėmis pamaišydami, kad duona gerai sugertų skystį."
      }
    },
    {
      "step": 5,
      "name": {
        "lt": "Formavimas"
      },
      "text": {
        "lt": "Drėgna audekle susukite masę į 6 cm storio ritinį ir tvirtai užriškite galus."
      }
    },
    {
      "step": 6,
      "name": {
        "lt": "Virimimas"
      },
      "text": {
        "lt": "Dideliame puode užvirinkite vandenį su druska ir atsargiai įdėkite paruoštą ritinį. Virkite 40 minučių."
      }
    },
    {
      "step": 7,
      "name": {
        "lt": "Pateikimas"
      },
      "text": {
        "lt": "Ištraukite iš vandens, nuimkite audeklą ir pjaustykite 1 cm storio riekelėmis."
      }
    }
  ],
  "notes": [
    {
      "text": {
        "lt": "Galima naudoti aliejų vietoj sviesto."
      },
      "priority": 1
    },
    {
      "text": {
        "lt": "Pjaustymui naudokite medinę lentą, kad sugertų drėgmę."
      },
      "priority": 2
    }
