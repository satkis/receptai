{
  "version": 2,

  "github": {
    "autoAlias": false
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_SITE_URL": "https://ragaujam.lt"
    }
  },
  "functions": {
    "pages/api/**/*.ts": {
      "maxDuration": 30
    },
    "pages/sitemap*.tsx": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "redirects": []
}
